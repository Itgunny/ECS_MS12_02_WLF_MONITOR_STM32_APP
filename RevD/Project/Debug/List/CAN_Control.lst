    411          void CAN_TX_Data(u8* Data)
    412          {
    413          	//u8 TransmitMailbox;
    414          	CanTxMsg TxMessage;
    415          	
    416          	TxMessage.ExtId=((unsigned long)(Identifier.Priority) << 26)+((unsigned long)(Identifier.Data_Page) << 24)+((unsigned long)(Identifier.PDU_Format) << 16)
    417          				                      + ((unsigned long)(Identifier.PDU_Specific) << 8) + ((unsigned long)(Identifier.Source_Address) << 0) ;
    418          			
    419          	TxMessage.IDE = CAN_ID_EXT;
    420          	TxMessage.RTR = CAN_RTR_DATA;
    421          	TxMessage.DLC = 8;
    422          
    423          	memcpy((u8*)&TxMessage.Data[0], (u8*)&Data[0], 8);
    424          
    425          	
    426          	CAN_Transmit(CAN1,&TxMessage);
    427          }
