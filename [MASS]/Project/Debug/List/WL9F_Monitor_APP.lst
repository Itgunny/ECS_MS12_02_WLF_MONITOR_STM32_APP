    588          void System_CheckPowerIG()
    589          {
    590          	// Check POWER IG
    591          	if(!WL9FM_GetPowerIG())
    592          	{
    593          		WL9FM_PowerIG(PowerIG_ON);
    594          		SystemReset = 0;
    595          	}
    596          	else
    597          	{
    598          		if(E2PROM_Save == 0)
    599          		{
    600          			//SaveDataToEEPROM();
    601          
    602          			PwrOffCnt = 0;
    603          			
    604          			while(1)
    605          			{
    606          				//	100msec 마다 15번 체크 -> 1.5초..
    607          				//	PowerIG Off 상태일 경우
    608          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    609          				//	PowerIG On  상태일 경우 
    610          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    611          				
    612          				if(!WL9FM_GetPowerIG())
    613          				{
    614          					
    615          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    616          					
    617          					
    618          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    619          					SystemReset = 0;
    620          					return;
    621          				}
    622          			
    623          				TimeDelay_msec(100);
    624          
    625          				PwrOffCnt++;
    626          				if(PwrOffCnt >= 15)	
    627          					break;
    628          			}
    629          
    630          			// ++, 161021 bwk
    631          			if(SaveSmartKeyUse != SmartKeyUse)
    632          			{
    633          				SaveSMKUseToFlash(SaveSmartKeyUse);
    634          			}
    635          			// --, 161021 bwk
    636          			Buzzer_Off();
    637          
    638          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    639          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(100);
    640          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	//TimeDelay_msec(3);
    641          
    642          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    643          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    644          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    645          
    646          
    647          			Buzzer_UnLimitOff();
    648          			
    649          			//	POWER OFF 일 때, LAMP Clear
    650          			LAMP_Update_Data = LAMP_ALL_OFF;
    651          
    652          			//Old_LAMP_Update_Data = LAMP_ALL_OFF;
    653          
    654          			Illumination_Sig = 0;
    655          			Lamp_Update_System();
    656          			
    657          			//	엔딩화면 딜레이 시간
    658          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    659          			TimeDelay_msec(1200);
    660          
    661          			//	LCDBL, LCD, LED Off
    662          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    663          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    664          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    665          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    666          
    667          		//	++, kutelf, 140801
    668          		//	RevD.01.01 
    669          		//	Power, PMIC On/Off 방법 변경 
    670          		#if 0
    671          
    672          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    673          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    674          			WL9FM_EXYNOS_PMIC_nRESET();
    675          		#else
    676          			WL9FM_EXYNOS_PMIC_PWROFF();
    677          			
    678          			//	Exynos VDD5V0_4412 Off
    679          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    680          			
    681          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    682          
    683          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    684          			if(!WL9FM_GetPowerIG())
    685          			{
    686          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    687          
    688          				//	++, kutelf, 150810
    689          				#if 1
    690          				if (Flag_St_Update==1)
    691          				{
    692          					gRebootCmd=1;
    693          					Flag_St_Update=0;
    694          
    695          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    696          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    697          
    698          					Software_Reset_BootLoader();
    699          				}
    700          				else
    701          				{
    702          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    703          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    704          
    705          					Software_Reset_Application();
    706          				}	
    707          				#else
    708          				if(Flag_St_Update==1)
    709          				{
    710          					gRebootCmd=1;
    711          					Flag_St_Update=0;
    712          				}
    713          				else
    714          					SystemReset = 1;			//	Software RESET
    715          
    716          				USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    717          				CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    718          				return;
    719          				#endif
    720          				//	--, kutelf, 150810
    721          			}
    722          
    723          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	 
    724          			WL9FM_PowerIG(PowerIG_OFF);		
    725          			WL9FM_PowerIG(PowerIG_OFF);		
    726          		#endif
    727          		//	--, kutelf, 140801
    728          		}
    729          	}
