   1049          void CheckResponseMsgComm(void)
   1050          {
   1051          	u8 output_buf[8];
   1052          	u16 upper_cpk, lower_cpk;
   1053          
   1054          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
   1055          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
   1056          	
   1057          	setCipherParam(upper_cpk, lower_cpk);
   1058          	//setCipherParam(0x1122, 0x3344);	// 임시
   1059          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
   1060          
   1061          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
   1062          
   1063          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
   1064          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
   1065          	{
   1066          		if(recv_smartkey.Smk_Response_Code == 1)
   1067          		{
   1068          			switch(recv_smartkey.Smk_Response_Flag)
   1069          			{
   1070          				
   1071          				case 2 :
   1072          					SendSMKMsgResult(SMK_MSG_TAG_NO_REALIZE);
   1073          					break;
   1074          				
   1075          				case 1:
   1076          				case 3 :
   1077          					SendSMKMsgResult(SMK_MSG_FAIL);
   1078          					break;
   1079          				case 4 :
   1080          					SendSMKMsgResult(SMK_MSG_TAG_ALREADY_REG);
   1081          					break;
   1082          				default :
   1083          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
   1084          					{
   1085          						if(recv_smartkey.Registered_Tag_Count == 1)
   1086          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
   1087          						else if(recv_smartkey.Registered_Tag_Count == 2)
   1088          							SendSMKMsgResult(SMK_MSG_TAG_SECOND_REG_SUCCESS);
   1089          						else if(recv_smartkey.Registered_Tag_Count == 3)
   1090          							SendSMKMsgResult(SMK_MSG_TAG_THIRD_REG_SUCCESS);
   1091          						else if(recv_smartkey.Registered_Tag_Count == 4)
   1092          							SendSMKMsgResult(SMK_MSG_TAG_FOURTH_REG_SUCCESS);
   1093          						else if(recv_smartkey.Registered_Tag_Count == 5)
   1094          							SendSMKMsgResult(SMK_MSG_TAG_FIFTH_REG_SUCCESS);
   1095          						else
   1096          							SendSMKMsgResult(SMK_MSG_FAIL);
   1097          					}
   1098          					else
   1099          					{
   1100          						// ++, 150707 sys
   1101          						#if 0
   1102          						if(SMK_Tag_Count != 0)
   1103          							SMK_Tag_Count--;
   1104          						#endif
   1105          						// --, 150707 sys
   1106          						
   1107          						SendSMKMsgResult(SMK_MSG_FAIL);
   1108          					}
   1109          					break;
   1110          			}
   1111          		}
   1112          		else if(recv_smartkey.Smk_Response_Code == 2)
   1113          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
   1114          		else
   1115          			SendSMKMsgResult(SMK_MSG_FAIL);
   1116          	}
