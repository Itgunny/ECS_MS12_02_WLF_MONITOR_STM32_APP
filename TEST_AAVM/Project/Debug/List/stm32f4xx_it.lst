    512          		|| (Iden.Source_Address == 0x00)|| (Iden.Source_Address == 0x03) || (Iden.Source_Address == 0x02) || (Iden.Source_Address == 0x19)
    513          		|| (Iden.Source_Address == 0x34) || (Iden.Source_Address == 0xDE))
    514          		{
    515          		#if 1
    516          			
    517          			if(CANUpdateFlag == 0)
    518          			{
    519          				PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    520          				if((PF == 254) || (PF == 255) || (PF == 239) )
    521          				{	
    522          					
    523          					if(Iden.PDU_Specific == 232)	// Smart Key
    524          					{
    525          						smk_flag_data.recv_resp_packet |= RESPONSE_AUTHENTICATION;	
    526          						//smk_flag_data.recv_resp_packet |= 0x0100; 
    527          						memcpy((u8*)&recv_smartkey, (u8*)&RxMsg.Data[0], 8);
    528          					}
    529          					else
    530          					{
    531          		
    532          						//if(Iden.PDU_Specific == 0x9b)
    533          							CAN_RX(RxMsg);
    534          					}
    535          				}
    536          				
    537          				else
    538          				{                    
    539          	                		CAN_RX(RxMsg);
