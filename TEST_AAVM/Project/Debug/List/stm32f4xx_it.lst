    422          /**
    423            * @brief  This function handles SysTick Handler.
    424            * @param  None
    425            * @retval None
    426            */
    427          void SysTick_Handler(void)
    428          {
    429            	TimeDelay_Decrement();
    430          
    431              ++WL9FM_TIME.Cnt_1mSec;
    432          
    433          	if (WL9FM_TIME.Cnt_1mSec % 1 == 0)                   //  1msec
    434          	{
    435                  WL9FM_TIME.Flag_1mSec = 1;
    436          
    437          		if (WL9FM_TIME.Cnt_1mSec % 10 == 0)              //  10msec
    438          		{
    439          			WL9FM_TIME.Flag_10mSec = 1;
    440          			// ++, 160511 bwk
    441          			if(Hardware_Revision >= REVH)
    442          				CPU_OK();
    443          			// --, 160511 bwk
    444          
    445          			if (WL9FM_TIME.Cnt_1mSec % 100 == 0)         //  100msec
    446          			{
    447          				WL9FM_TIME.Flag_100mSec = 1;
    448          
    449          				if(WL9FM_TIME.Cnt_1mSec % 500 == 0)			// 500msec
    450          				{
    451          					WL9FM_TIME.Flag_500mSec = 1;
    452          					if (WL9FM_TIME.Cnt_1mSec % 1000 == 0)    //  1000 msec
    453          					{
    454          						WL9FM_TIME.Flag_1Sec = 1;
    455          						WL9FM_TIME.Cnt_1mSec = 0;
    456          					}
    457          				}
    458          
    459          				
    460          			}
    461          		}
    462          	}    
    463          }
