    551          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    552          					{
    553          						CAN_RX(RxMsg);
    554          					}
    555          
    556          				}
    557          				if(Iden.Source_Address == 0x47)
    558          				{
    559          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    560          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    561          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    562          					
    563          					if(PGN == 0xEF28 && RxMsg.Data[0] == 122)
    564          					{
    565          						CAN_RX(RxMsg);
    566          					}
    567          				}
    568          				if(Iden.Source_Address == 0x34){
    569          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    570          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    571          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    572          					if(PGN == 0xFFFA || PGN == 0xFE34){
    573          						CAN_RX(RxMsg);
    574          					}
    575          				}
    576          			}
    577          		#else
    578          			if(CANUpdateFlag == 1)
    579          			{
    580          				if(Iden.Source_Address == CANUpdateSA)
    581          				{
    582          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    583          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    584          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    585          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    586          					{
    587          						//OperateRingBuffer();
    588          						CAN_RX(RxMsg);
    589          						CANRXIndex++;
    590          					}
    591          
    592          				}
    593          				
    594          			}
    595          		#endif
    596          			
    597          			
    598          
    599          			
    600          			
    601          		}
    602          
    603          }
