    563          void System_CheckPowerIG()
    564          {
    565          	// Check POWER IG
    566          	if(!WL9FM_GetPowerIG())
    567          	{
    568          		WL9FM_PowerIG(PowerIG_ON);
    569          		SystemReset = 0;
    570          	}
    571          	else
    572          	{
    573          		if(E2PROM_Save == 0)
    574          		{
    575          			//SaveDataToEEPROM();
    576          
    577          			PwrOffCnt = 0;
    578          			
    579          			while(1)
    580          			{
    581          				//	100msec 마다 15번 체크 -> 1.5초..
    582          				//	PowerIG Off 상태일 경우
    583          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    584          				//	PowerIG On  상태일 경우 
    585          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    586          				
    587          				if(!WL9FM_GetPowerIG())
    588          				{
    589          					
    590          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    591          					
    592          					
    593          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    594          					SystemReset = 0;
    595          					return;
    596          				}
    597          			
    598          				TimeDelay_msec(100);
    599          
    600          				PwrOffCnt++;
    601          				if(PwrOffCnt >= 15)	
    602          					break;
    603          			}
    604          
    605          			Buzzer_Off();
    606          
    607          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    608          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(100);
    609          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	//TimeDelay_msec(3);
    610          
    611          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    612          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    613          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    614          
    615          
    616          			Buzzer_UnLimitOff();
    617          			
    618          			//	POWER OFF 일 때, LAMP Clear
    619          			LAMP_Update_Data = LAMP_ALL_OFF;
    620          
    621          			//Old_LAMP_Update_Data = LAMP_ALL_OFF;
    622          
    623          			Illumination_Sig = 0;
    624          			Lamp_Update_System();
    625          			
    626          			//	엔딩화면 딜레이 시간
    627          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    628          			TimeDelay_msec(1200);
    629          
    630          			//	LCDBL, LCD, LED Off
    631          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    632          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    633          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    634          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    635          
    636          		//	++, kutelf, 140801
    637          		//	RevD.01.01 
    638          		//	Power, PMIC On/Off 방법 변경 
    639          		#if 0
    640          
    641          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    642          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    643          			WL9FM_EXYNOS_PMIC_nRESET();
    644          		#else
    645          			WL9FM_EXYNOS_PMIC_PWROFF();
    646          			
    647          			//	Exynos VDD5V0_4412 Off
    648          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    649          			
    650          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    651          
    652          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    653          			if(!WL9FM_GetPowerIG())
    654          			{
    655          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    656          
    657          				//	++, kutelf, 150810
    658          				#if 1
    659          				if (Flag_St_Update==1)
    660          				{
    661          					gRebootCmd=1;
    662          					Flag_St_Update=0;
    663          
    664          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    665          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    666          
    667          					Software_Reset_BootLoader();
    668          				}
    669          				else
    670          				{
    671          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    672          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    673          
    674          					Software_Reset_Application();
    675          				}	
    676          				#else
    677          				if(Flag_St_Update==1)
    678          				{
    679          					gRebootCmd=1;
    680          					Flag_St_Update=0;
    681          				}
    682          				else
    683          					SystemReset = 1;			//	Software RESET
    684          
    685          				USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    686          				CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    687          				return;
    688          				#endif
    689          				//	--, kutelf, 150810
    690          			}
    691          
    692          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	 
    693          			WL9FM_PowerIG(PowerIG_OFF);		
    694          			WL9FM_PowerIG(PowerIG_OFF);		
    695          		#endif
    696          		//	--, kutelf, 140801
    697          		}
    698          	}
    699          }
