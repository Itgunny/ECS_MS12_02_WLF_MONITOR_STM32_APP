    718          				CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    719          				return;
    720          				#endif
    721          				//	--, kutelf, 150810
    722          			}
    723          
    724          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	 
    725          			WL9FM_PowerIG(PowerIG_OFF);		
    726          			WL9FM_PowerIG(PowerIG_OFF);		
    727          		#endif
    728          		//	--, kutelf, 140801
