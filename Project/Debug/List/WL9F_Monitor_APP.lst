    755          u8 CheckResponseAuthentication(u8 Auth)
    756          {
    757          	u8 output_buf[8];
    758          	u16 upper_cpk, lower_cpk;
    759          
    760          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    761          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    762          	
    763          	setCipherParam(upper_cpk, lower_cpk);
    764          	//setCipherParam(0x1122, 0x3344);	// 임시
    765          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    766          
    767          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    768          	
    769          	
    770          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    771          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    772          	{
    773          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    774          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    775          		{
    776          			return SMK_RESPONSE_FLAG_SUCCESS;
    777          		}
    778          		else
    779          			return recv_smartkey.Smk_Response_Flag;
    780          	}
    781          	else
    782          		return SMK_RESPONSE_CODE_NOT_MATCH;
    783          }
