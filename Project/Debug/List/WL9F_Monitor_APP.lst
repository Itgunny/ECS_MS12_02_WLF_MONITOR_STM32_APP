    814          					break;
    815          				default :
    816          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
    817          					{
    818          						if(recv_smartkey.Registered_Tag_Count == 1)
    819          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
    820          						else if(recv_smartkey.Registered_Tag_Count == 2)
