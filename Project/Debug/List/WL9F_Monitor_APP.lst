    794          					break;
    795          				default :
    796          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
    797          					{
    798          						if(recv_smartkey.Registered_Tag_Count == 1)
    799          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
    800          						else if(recv_smartkey.Registered_Tag_Count == 2)
