    999          			SMK_Msg_Send = 1;
   1000          			smk_flag_data.recv_resp_packet = 0;
   1001          		//	Uart2_RxMsg_Smk_Reg_Eli[7] = Uart2_RxMsg_Smk_Reg_Eli[0];
   1002          			Uart2_RxMsg_Smk_Reg_Eli[2] = 0xff;
   1003          		}
   1004          
   1005          		if(SMK_Msg_Send == 1)
   1006          		{
   1007          			if(smk_flag_data.recv_resp_packet == RESPONSE_AUTHENTICATION)
