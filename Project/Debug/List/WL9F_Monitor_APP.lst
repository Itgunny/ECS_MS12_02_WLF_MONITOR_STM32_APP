    488          void System_CheckPowerIG()
    489          {
    490          	// Check POWER IG
    491          	if(!WL9FM_GetPowerIG())
    492          	{
    493          		WL9FM_PowerIG(PowerIG_ON);
    494          		SystemReset = 0;
    495          	}
    496          	else
    497          	{
    498          		if(E2PROM_Save == 0)
    499          		{
    500          			//SaveDataToEEPROM();
    501          
    502          			PwrOffCnt = 0;
    503          			
    504          			while(1)
    505          			{
    506          				//	100msec 마다 15번 체크 -> 1.5초..
    507          				//	PowerIG Off 상태일 경우
    508          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    509          				//	PowerIG On  상태일 경우 
    510          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    511          				
    512          				if(!WL9FM_GetPowerIG())
    513          				{
    514          					
    515          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    516          					
    517          					
    518          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    519          					SystemReset = 0;
    520          					return;
    521          				}
    522          			
    523          				TimeDelay_msec(100);
    524          
    525          				PwrOffCnt++;
    526          				if(PwrOffCnt >= 15)	
    527          					break;
    528          			}
    529          
    530          			Buzzer_Off();
    531          
    532          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    533          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    534          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    535          
    536          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    537          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    538          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    539          
    540          			//	POWER OFF 일 때, LAMP Clear
    541          			LAMP_Update_Data = LAMP_ALL_OFF;
    542          			Old_LAMP_Update_Data = LAMP_ALL_OFF;
    543          		 	Illumination_Sig = 0;
    544          			Lamp_Update_System();
    545          			
    546          			//	엔딩화면 딜레이 시간
    547          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    548          			TimeDelay_msec(1200);
    549          
    550          			//	LCDBL, LCD, LED Off
    551          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    552          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    553          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    554          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    555          
    556          		//	++, kutelf, 140801
    557          		//	RevD.01.01 
    558          		//	Power, PMIC On/Off 방법 변경 
    559          		#if 0
    560          
    561          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    562          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    563          			WL9FM_EXYNOS_PMIC_nRESET();
    564          		#else
    565          			WL9FM_EXYNOS_PMIC_PWROFF();
    566          			
    567          			//	Exynos VDD5V0_4412 Off
    568          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    569          			
    570          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    571          
    572          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    573          			if(!WL9FM_GetPowerIG())
    574          			{
    575          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    576          				SystemReset = 1;			//	Software RESET
    577          
    578          				USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    579          				CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    580          				return;
    581          			}
    582          
    583          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	    
    584          		#endif
    585          		//	--, kutelf, 140801
    586          		}
    587          	}
