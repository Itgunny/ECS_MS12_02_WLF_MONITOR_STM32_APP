   1051          void CheckResponseMsgComm(void)
   1052          {
   1053          	u8 output_buf[8];
   1054          	u16 upper_cpk, lower_cpk;
   1055          
   1056          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
   1057          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
   1058          	
   1059          	setCipherParam(upper_cpk, lower_cpk);
   1060          	//setCipherParam(0x1122, 0x3344);	// 임시
   1061          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
   1062          
   1063          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
   1064          
   1065          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
   1066          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
   1067          	{
   1068          		if(recv_smartkey.Smk_Response_Code == 1)
   1069          		{
   1070          			switch(recv_smartkey.Smk_Response_Flag)
   1071          			{
   1072          				
   1073          				case 2 :
   1074          					SendSMKMsgResult(SMK_MSG_TAG_NO_REALIZE);
   1075          					break;
   1076          				
   1077          				case 1:
   1078          				case 3 :
   1079          					SendSMKMsgResult(SMK_MSG_FAIL);
   1080          					break;
   1081          				case 4 :
   1082          					SendSMKMsgResult(SMK_MSG_TAG_ALREADY_REG);
   1083          					break;
   1084          				default :
   1085          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
   1086          					{
   1087          						if(recv_smartkey.Registered_Tag_Count == 1)
   1088          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
   1089          						else if(recv_smartkey.Registered_Tag_Count == 2)
   1090          							SendSMKMsgResult(SMK_MSG_TAG_SECOND_REG_SUCCESS);
   1091          						else if(recv_smartkey.Registered_Tag_Count == 3)
   1092          							SendSMKMsgResult(SMK_MSG_TAG_THIRD_REG_SUCCESS);
   1093          						else if(recv_smartkey.Registered_Tag_Count == 4)
   1094          							SendSMKMsgResult(SMK_MSG_TAG_FOURTH_REG_SUCCESS);
   1095          						else if(recv_smartkey.Registered_Tag_Count == 5)
   1096          							SendSMKMsgResult(SMK_MSG_TAG_FIFTH_REG_SUCCESS);
   1097          						else
   1098          							SendSMKMsgResult(SMK_MSG_FAIL);
   1099          					}
   1100          					else
   1101          					{
   1102          						// ++, 150707 sys
   1103          						#if 0
   1104          						if(SMK_Tag_Count != 0)
   1105          							SMK_Tag_Count--;
   1106          						#endif
   1107          						// --, 150707 sys
   1108          						
   1109          						SendSMKMsgResult(SMK_MSG_FAIL);
   1110          					}
   1111          					break;
   1112          			}
   1113          		}
   1114          		else if(recv_smartkey.Smk_Response_Code == 2)
   1115          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
   1116          		else
   1117          			SendSMKMsgResult(SMK_MSG_FAIL);
   1118          	}
   1119          	else
   1120          		SendSMKMsgResult(SMK_MSG_FAIL);
   1121          }
