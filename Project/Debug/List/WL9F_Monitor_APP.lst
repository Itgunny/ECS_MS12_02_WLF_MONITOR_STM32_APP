    735          u8 CheckResponseAuthentication(u8 Auth)
    736          {
    737          	u8 output_buf[8];
    738          	u16 upper_cpk, lower_cpk;
    739          
    740          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    741          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    742          	
    743          	setCipherParam(upper_cpk, lower_cpk);
    744          	//setCipherParam(0x1122, 0x3344);	// 임시
    745          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    746          
    747          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    748          	
    749          	
    750          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    751          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    752          	{
    753          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    754          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    755          		{
    756          			return SMK_RESPONSE_FLAG_SUCCESS;
    757          		}
    758          		else
    759          			return recv_smartkey.Smk_Response_Flag;
    760          	}
    761          	else
    762          		return SMK_RESPONSE_CODE_NOT_MATCH;
    763          }
