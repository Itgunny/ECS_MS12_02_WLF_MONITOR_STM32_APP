   1089          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
   1090          			{
   1091          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
   1092          				RequestSecondAuthentication();
   1093          			}
