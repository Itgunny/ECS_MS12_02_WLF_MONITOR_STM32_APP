    796          void CheckResponseMsgComm(void)
    797          {
    798          	u8 output_buf[8];
    799          	u16 upper_cpk, lower_cpk;
    800          
    801          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    802          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    803          	
    804          	setCipherParam(upper_cpk, lower_cpk);
    805          	//setCipherParam(0x1122, 0x3344);	// 임시
    806          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    807          
    808          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    809          
    810          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    811          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    812          	{
    813          		if(recv_smartkey.Smk_Response_Code == 1)
    814          		{
    815          			switch(recv_smartkey.Smk_Response_Flag)
    816          			{
    817          				case 2 :
    818          					SendSMKMsgResult(SMK_MSG_TAG_NO_REALIZE);
    819          					break;
    820          				case 3 :
    821          					SendSMKMsgResult(SMK_MSG_FAIL);
    822          					break;
    823          				case 4 :
    824          					SendSMKMsgResult(SMK_MSG_TAG_ALREADY_REG);
    825          					break;
    826          				default :
    827          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
    828          					{
    829          						if(recv_smartkey.Registered_Tag_Count == 1)
    830          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
    831          						else if(recv_smartkey.Registered_Tag_Count == 2)
    832          							SendSMKMsgResult(SMK_MSG_TAG_SECOND_REG_SUCCESS);
    833          						else
    834          							SendSMKMsgResult(SMK_MSG_FAIL);
    835          					}
    836          					else
    837          					{
    838          						if(SMK_Tag_Count != 0)
    839          							SMK_Tag_Count--;
    840          
    841          						SendSMKMsgResult(SMK_MSG_FAIL);
    842          					}
    843          					break;
    844          			}
    845          			
    846          		}
    847          		else if(recv_smartkey.Smk_Response_Code == 2)
    848          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
    849                          else
    850                                  SendSMKMsgResult(SMK_MSG_FAIL);
    851          	}
    852          	else
    853          		SendSMKMsgResult(SMK_MSG_FAIL);
    854          }
