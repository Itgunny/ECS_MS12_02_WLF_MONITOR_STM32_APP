   1006          void SmartKeyAuthentication(void)
   1007          {
   1008          	u8  Auth = 0;
   1009          	u8 returnvalue;
   1010          
   1011          	if(AuthResult != 1)
   1012          	{
   1013          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
   1014          		{
   1015          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
   1016          			{
   1017          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
   1018          				RequestFirstAuthentication();
   1019          			}
   1020          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
   1021          			{
   1022          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
   1023          				RequestSecondAuthentication();
   1024          			}
   1025          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
   1026          			{
   1027          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
