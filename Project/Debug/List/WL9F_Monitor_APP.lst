    825          					else
    826          					{
    827          						if(SMK_Tag_Count != 0)
    828          							SMK_Tag_Count--;
    829          
    830          						SendSMKMsgResult(SMK_MSG_FAIL);
    831          					}
    832          					break;
    833          			}
    834          			
    835          		}
    836          		else if(recv_smartkey.Smk_Response_Code == 2)
    837          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
    838                          else
    839                                  SendSMKMsgResult(SMK_MSG_FAIL);
    840          	}
    841          	else
    842          		SendSMKMsgResult(SMK_MSG_FAIL);
    843          }
