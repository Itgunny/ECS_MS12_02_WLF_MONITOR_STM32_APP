   1050          void CheckResponseMsgComm(void)
   1051          {
   1052          	u8 output_buf[8];
   1053          	u16 upper_cpk, lower_cpk;
   1054          
   1055          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
   1056          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
   1057          	
   1058          	setCipherParam(upper_cpk, lower_cpk);
   1059          	//setCipherParam(0x1122, 0x3344);	// 임시
   1060          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
   1061          
   1062          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
   1063          
   1064          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
   1065          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
   1066          	{
   1067          		if(recv_smartkey.Smk_Response_Code == 1)
   1068          		{
   1069          			switch(recv_smartkey.Smk_Response_Flag)
   1070          			{
   1071          				
   1072          				case 2 :
   1073          					SendSMKMsgResult(SMK_MSG_TAG_NO_REALIZE);
   1074          					break;
   1075          				
   1076          				case 1:
   1077          				case 3 :
   1078          					SendSMKMsgResult(SMK_MSG_FAIL);
   1079          					break;
   1080          				case 4 :
   1081          					SendSMKMsgResult(SMK_MSG_TAG_ALREADY_REG);
   1082          					break;
   1083          				default :
   1084          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
   1085          					{
   1086          						if(recv_smartkey.Registered_Tag_Count == 1)
   1087          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
   1088          						else if(recv_smartkey.Registered_Tag_Count == 2)
   1089          							SendSMKMsgResult(SMK_MSG_TAG_SECOND_REG_SUCCESS);
   1090          						else if(recv_smartkey.Registered_Tag_Count == 3)
   1091          							SendSMKMsgResult(SMK_MSG_TAG_THIRD_REG_SUCCESS);
   1092          						else if(recv_smartkey.Registered_Tag_Count == 4)
   1093          							SendSMKMsgResult(SMK_MSG_TAG_FOURTH_REG_SUCCESS);
   1094          						else if(recv_smartkey.Registered_Tag_Count == 5)
   1095          							SendSMKMsgResult(SMK_MSG_TAG_FIFTH_REG_SUCCESS);
   1096          						else
   1097          							SendSMKMsgResult(SMK_MSG_FAIL);
   1098          					}
   1099          					else
   1100          					{
   1101          						// ++, 150707 sys
   1102          						#if 0
   1103          						if(SMK_Tag_Count != 0)
   1104          							SMK_Tag_Count--;
   1105          						#endif
   1106          						// --, 150707 sys
   1107          						
   1108          						SendSMKMsgResult(SMK_MSG_FAIL);
   1109          					}
   1110          					break;
   1111          			}
   1112          		}
   1113          		else if(recv_smartkey.Smk_Response_Code == 2)
   1114          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
   1115          		else
   1116          			SendSMKMsgResult(SMK_MSG_FAIL);
   1117          	}
   1118          	else
   1119          		SendSMKMsgResult(SMK_MSG_FAIL);
   1120          }
