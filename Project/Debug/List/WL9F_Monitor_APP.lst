    766          u8 CheckResponseAuthentication(u8 Auth)
    767          {
    768          	u8 output_buf[8];
    769          	u16 upper_cpk, lower_cpk;
    770          
    771          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    772          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    773          	
    774          	setCipherParam(upper_cpk, lower_cpk);
    775          	//setCipherParam(0x1122, 0x3344);	// 임시
    776          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    777          
    778          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    779          	
    780          	
    781          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    782          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    783          	{
    784          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    785          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    786          		{
    787          			return SMK_RESPONSE_FLAG_SUCCESS;
    788          		}
    789          		else
    790          			return recv_smartkey.Smk_Response_Flag;
    791          	}
    792          	else
    793          		return SMK_RESPONSE_CODE_NOT_MATCH;
    794          }
