    202          void Software_Reset_Application(void)
    203          { 	/* Jump to user application */
    204          		JumpAddress_Cluster_App = *(__IO uint32_t*) (ApplicationAddress + 4);
    205          		Jump_To_Application_Cluster_App = (pFunction) JumpAddress_Cluster_App;
    206          		/* Initialize user application's Stack Pointer */
    207          		__set_MSP(*(__IO uint32_t*) ApplicationAddress);
    208          		Jump_To_Application_Cluster_App();
    209          }	
    210          
    211          void Software_Reset_BootLoader(void)
    212          { 	/* Jump to user application */
    213          		JumpAddress_Cluster_Iap = *(__IO uint32_t*) (BootLoaderAddress + 4);
    214          		Jump_To_Application_Cluster_Iap = (pFunction) JumpAddress_Cluster_Iap;
    215          		/* Initialize user application's Stack Pointer */
    216          		__set_MSP(*(__IO uint32_t*) BootLoaderAddress);
    217          		Jump_To_Application_Cluster_Iap();
    218          }	
