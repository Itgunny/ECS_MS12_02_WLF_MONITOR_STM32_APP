    984          					}
    985          					break;
    986          			}
    987          		}
    988          		else if(recv_smartkey.Smk_Response_Code == 2)
    989          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
    990          		else
