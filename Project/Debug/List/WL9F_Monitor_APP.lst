    804          					}
    805          					else
    806          					{
    807          						if(SMK_Tag_Count != 0)
    808          							SMK_Tag_Count--;
    809          
    810          						SendSMKMsgResult(SMK_MSG_FAIL);
    811          					}
    812          					break;
    813          			}
    814          			
    815          		}
    816          		else if(recv_smartkey.Smk_Response_Code == 2)
    817          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
    818                          else
    819                                  SendSMKMsgResult(SMK_MSG_FAIL);
    820          	}
    821          	else
    822          		SendSMKMsgResult(SMK_MSG_FAIL);
    823          }
