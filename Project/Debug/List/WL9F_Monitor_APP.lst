   1016          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
   1017          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
   1018          	{
   1019          		if(recv_smartkey.Smk_Response_Code == 1)
   1020          		{
   1021          			switch(recv_smartkey.Smk_Response_Flag)
