    203          void Software_Reset_Application(void)
    204          { 	/* Jump to user application */
    205          		JumpAddress_Cluster_App = *(__IO uint32_t*) (ApplicationAddress + 4);
    206          		Jump_To_Application_Cluster_App = (pFunction) JumpAddress_Cluster_App;
    207          		/* Initialize user application's Stack Pointer */
    208          		__set_MSP(*(__IO uint32_t*) ApplicationAddress);
    209          		Jump_To_Application_Cluster_App();
    210          }	
    211          
    212          void Software_Reset_BootLoader(void)
    213          { 	/* Jump to user application */
    214          		JumpAddress_Cluster_Iap = *(__IO uint32_t*) (BootLoaderAddress + 4);
    215          		Jump_To_Application_Cluster_Iap = (pFunction) JumpAddress_Cluster_Iap;
    216          		/* Initialize user application's Stack Pointer */
    217          		__set_MSP(*(__IO uint32_t*) BootLoaderAddress);
    218          		Jump_To_Application_Cluster_Iap();
    219          }	
