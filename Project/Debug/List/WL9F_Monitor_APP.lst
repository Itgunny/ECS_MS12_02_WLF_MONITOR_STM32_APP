    558          void System_CheckPowerIG()
    559          {
    560          	// Check POWER IG
    561          	if(!WL9FM_GetPowerIG())
    562          	{
    563          		WL9FM_PowerIG(PowerIG_ON);
    564          		SystemReset = 0;
    565          	}
    566          	else
    567          	{
    568          		if(E2PROM_Save == 0)
    569          		{
    570          			//SaveDataToEEPROM();
    571          
    572          			PwrOffCnt = 0;
    573          			
    574          			while(1)
    575          			{
    576          				//	100msec 마다 15번 체크 -> 1.5초..
    577          				//	PowerIG Off 상태일 경우
    578          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    579          				//	PowerIG On  상태일 경우 
    580          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    581          				
    582          				if(!WL9FM_GetPowerIG())
    583          				{
    584          					
    585          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    586          					
    587          					
    588          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    589          					SystemReset = 0;
    590          					return;
    591          				}
    592          			
    593          				TimeDelay_msec(100);
    594          
    595          				PwrOffCnt++;
    596          				if(PwrOffCnt >= 15)	
    597          					break;
    598          			}
    599          
    600          			Buzzer_Off();
    601          
    602          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    603          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(100);
    604          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	//TimeDelay_msec(3);
    605          
    606          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    607          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    608          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    609          
    610          
    611          			Buzzer_UnLimitOff();
    612          			
    613          			//	POWER OFF 일 때, LAMP Clear
    614          			LAMP_Update_Data = LAMP_ALL_OFF;
    615          
    616          			//Old_LAMP_Update_Data = LAMP_ALL_OFF;
    617          
    618          			Illumination_Sig = 0;
    619          			Lamp_Update_System();
    620          			
    621          			//	엔딩화면 딜레이 시간
    622          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    623          			TimeDelay_msec(1200);
    624          
    625          			//	LCDBL, LCD, LED Off
    626          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    627          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    628          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    629          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    630          
    631          		//	++, kutelf, 140801
    632          		//	RevD.01.01 
    633          		//	Power, PMIC On/Off 방법 변경 
    634          		#if 0
    635          
    636          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    637          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    638          			WL9FM_EXYNOS_PMIC_nRESET();
    639          		#else
    640          			WL9FM_EXYNOS_PMIC_PWROFF();
    641          			
    642          			//	Exynos VDD5V0_4412 Off
    643          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    644          			
    645          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    646          
    647          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    648          			if(!WL9FM_GetPowerIG())
    649          			{
    650          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    651          
    652          				//	++, kutelf, 150810
    653          				#if 1
    654          				if (Flag_St_Update==1)
    655          				{
    656          					gRebootCmd=1;
    657          					Flag_St_Update=0;
    658          
    659          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    660          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    661          
    662          					Software_Reset_BootLoader();
    663          				}
    664          				else
    665          				{
    666          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    667          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    668          
    669          					Software_Reset_Application();
    670          				}	
    671          				#else
    672          				if(Flag_St_Update==1)
    673          				{
    674          					gRebootCmd=1;
    675          					Flag_St_Update=0;
    676          				}
    677          				else
    678          					SystemReset = 1;			//	Software RESET
    679          
    680          				USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    681          				CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    682          				return;
    683          				#endif
    684          				//	--, kutelf, 150810
    685          			}
    686          
    687          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	 
    688          			WL9FM_PowerIG(PowerIG_OFF);		
    689          			WL9FM_PowerIG(PowerIG_OFF);		
    690          		#endif
    691          		//	--, kutelf, 140801
    692          		}
    693          	}
    694          }
