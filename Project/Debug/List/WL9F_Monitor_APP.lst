    762          void CheckResponseMsgComm(void)
    763          {
    764          	u8 output_buf[8];
    765          	u16 upper_cpk, lower_cpk;
    766          
    767          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    768          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    769          	
    770          	setCipherParam(upper_cpk, lower_cpk);
    771          	//setCipherParam(0x1122, 0x3344);	// 임시
    772          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    773          
