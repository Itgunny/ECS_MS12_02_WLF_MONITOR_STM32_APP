    867          void CheckResponseMsgComm(void)
    868          {
    869          	u8 output_buf[8];
    870          	u16 upper_cpk, lower_cpk;
    871          
    872          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    873          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    874          	
    875          	setCipherParam(upper_cpk, lower_cpk);
    876          	//setCipherParam(0x1122, 0x3344);	// 임시
    877          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    878          
    879          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    880          
    881          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    882          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    883          	{
    884          		if(recv_smartkey.Smk_Response_Code == 1)
    885          		{
    886          			switch(recv_smartkey.Smk_Response_Flag)
    887          			{
    888          				
    889          				case 2 :
    890          					SendSMKMsgResult(SMK_MSG_TAG_NO_REALIZE);
    891          					break;
    892          				case 1:
    893          				case 3 :
    894          					SendSMKMsgResult(SMK_MSG_FAIL);
    895          					break;
    896          				case 4 :
    897          					SendSMKMsgResult(SMK_MSG_TAG_ALREADY_REG);
    898          					break;
    899          				default :
    900          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
    901          					{
    902          						if(recv_smartkey.Registered_Tag_Count == 1)
    903          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
    904          						else if(recv_smartkey.Registered_Tag_Count == 2)
    905          							SendSMKMsgResult(SMK_MSG_TAG_SECOND_REG_SUCCESS);
    906          						else if(recv_smartkey.Registered_Tag_Count == 3)
    907          							SendSMKMsgResult(SMK_MSG_TAG_THIRD_REG_SUCCESS);
    908          						else if(recv_smartkey.Registered_Tag_Count == 4)
    909          							SendSMKMsgResult(SMK_MSG_TAG_FOURTH_REG_SUCCESS);
    910          						else if(recv_smartkey.Registered_Tag_Count == 5)
    911          							SendSMKMsgResult(SMK_MSG_TAG_FIFTH_REG_SUCCESS);
    912          						else
    913          							SendSMKMsgResult(SMK_MSG_FAIL);
    914          					}
    915          					else
    916          					{
    917          						if(SMK_Tag_Count != 0)
    918          							SMK_Tag_Count--;
    919          
    920          						SendSMKMsgResult(SMK_MSG_FAIL);
    921          					}
    922          					break;
    923          			}
    924          		}
    925          		else if(recv_smartkey.Smk_Response_Code == 2)
    926          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
    927          		else
    928          			SendSMKMsgResult(SMK_MSG_FAIL);
    929          	}
    930          	else
    931          		SendSMKMsgResult(SMK_MSG_FAIL);
    932          }
