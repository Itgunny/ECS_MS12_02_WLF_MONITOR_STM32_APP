    817          u8 CheckResponseAuthentication(u8 Auth)
    818          {
    819          	u8 output_buf[8];
    820          	u16 upper_cpk, lower_cpk;
    821          
    822          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    823          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    824          	
    825          	setCipherParam(upper_cpk, lower_cpk);
    826          	//setCipherParam(0x1122, 0x3344);	// 임시
    827          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    828          
    829          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    830          	
    831          	
    832          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    833          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    834          	{
    835          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    836          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    837          		{
    838          			return SMK_RESPONSE_FLAG_SUCCESS;
    839          		}
    840          		else
    841          			return recv_smartkey.Smk_Response_Flag;
    842          	}
    843          	else
    844          		return SMK_RESPONSE_CODE_NOT_MATCH;
    845          }
