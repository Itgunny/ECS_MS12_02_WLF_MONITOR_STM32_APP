   1132          		{
   1133          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
   1134          			{
   1135          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
   1136          				RequestFirstAuthentication();
