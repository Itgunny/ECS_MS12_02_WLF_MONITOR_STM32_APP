    968          void SmartKeyAuthentication(void)
    969          {
    970          	u8  Auth = 0;
    971          	u8 returnvalue;
    972          
    973          	if(AuthResult != 1)
    974          	{
    975          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    976          		{
    977          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    978          			{
    979          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    980          				RequestFirstAuthentication();
    981          			}
    982          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    983          			{
    984          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    985          				RequestSecondAuthentication();
    986          			}
    987          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    988          			{
    989          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    990          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    991          
    992          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    993          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    994          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    995          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    996          					
    997          				returnvalue = CheckResponseAuthentication(Auth);
    998          					
    999          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
   1000          				{
   1001          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
   1002          					{
   1003          						gAuthentication_Cnt = 0;
   1004          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
   1005          					}
   1006          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
   1007          					{
   1008          						gAuthentication_Cnt = 0;
   1009          						AuthResult = 1;
   1010          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
   1011          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
   1012          						SMKSuccess = SMK_SUCCESS;
   1013          						SendSMKAuthResult(SMK_SUCCESS);
   1014          
   1015          						// ++ , 141118 sys3215
   1016          						Flag_ESL=1;
   1017          						// -- , 141118 sys3215
   1018          						
   1019          					}
   1020          				}
   1021          				else
   1022          				{
