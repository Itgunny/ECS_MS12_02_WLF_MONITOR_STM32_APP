    908          void SmartKeyAuthentication(void)
    909          {
    910          	u8  Auth = 0;
    911          	u8 returnvalue;
    912          
    913          	if(AuthResult != 1)
    914          	{
    915          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    916          		{
    917          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    918          			{
    919          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    920          				RequestFirstAuthentication();
    921          			}
    922          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    923          			{
    924          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    925          				RequestSecondAuthentication();
    926          			}
    927          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    928          			{
    929          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    930          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    931          
    932          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    933          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    934          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    935          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    936          					
    937          				returnvalue = CheckResponseAuthentication(Auth);
    938          					
    939          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
    940          				{
    941          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    942          					{
    943          						gAuthentication_Cnt = 0;
    944          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    945          					}
    946          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    947          					{
    948          						gAuthentication_Cnt = 0;
    949          						AuthResult = 1;
    950          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
    951          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
    952          						SMKSuccess = SMK_SUCCESS;
    953          						SendSMKAuthResult(SMK_SUCCESS);
    954          
    955          						// ++ , 141118 sys3215
    956          						Flag_ESL=1;
    957          						// -- , 141118 sys3215
    958          						
    959          					}
    960          				}
    961          				else
    962          				{
    963          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
    964          					{
    965          						//AuthResult = 2;
    966          						AuthResult = 1;
    967          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
    968          						SMKSuccess = SMK_FAIL;
    969          						SendSMKAuthResult(SMK_FAIL);
    970          						return;
    971          					}
    972          
    973          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    974          						smk_flag_data.recv_resp_packet = REQUEST_FIRST_AUTHENTICATION;
    975          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    976          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    977          				}
    978          			}
    979          			else if(smk_flag_data.recv_resp_packet == (REQUEST_FIRST_AUTHENTICATION | RESPONSE_WAIT))
    980          			{
    981          				//RequestFirstAuthentication();
    982          			}
    983          			else if(smk_flag_data.recv_resp_packet == (REQUEST_SECOND_AUTHENTICATION | RESPONSE_WAIT))
    984          			{
    985          				//RequestSecondAuthentication();
    986          			}
    987          		}
    988          	}
    989          	//else if(AuthResult == 1)
    990          	{
    991          		if((Uart2_RxMsg_Smk_Reg_Eli[2] == 1) || (Uart2_RxMsg_Smk_Reg_Eli[2] == 2)) 
    992          		{
    993          			RequestSMKMessage(Uart2_RxMsg_Smk_Reg_Eli[2]);
    994          			SMK_Msg_Send = 1;
    995          			smk_flag_data.recv_resp_packet = 0;
    996          		//	Uart2_RxMsg_Smk_Reg_Eli[7] = Uart2_RxMsg_Smk_Reg_Eli[0];
    997          			Uart2_RxMsg_Smk_Reg_Eli[2] = 0xff;
    998          		}
    999          
   1000          		if(SMK_Msg_Send == 1)
   1001          		{
   1002          			if(smk_flag_data.recv_resp_packet == RESPONSE_AUTHENTICATION)
   1003          			{
   1004          				SMK_Msg_Send = 0;
   1005          				CheckResponseMsgComm();
   1006          			}
   1007          		}
   1008          	}
   1009          }
