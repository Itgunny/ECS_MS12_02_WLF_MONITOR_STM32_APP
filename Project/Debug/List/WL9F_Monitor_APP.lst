    889          						if(SMK_Tag_Count != 0)
    890          							SMK_Tag_Count--;
    891          
    892          						SendSMKMsgResult(SMK_MSG_FAIL);
    893          					}
    894          					break;
    895          			}
    896          			
    897          		}
    898          		else if(recv_smartkey.Smk_Response_Code == 2)
    899          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
    900          		else
    901          			SendSMKMsgResult(SMK_MSG_FAIL);
    902          	}
    903          	else
    904          		SendSMKMsgResult(SMK_MSG_FAIL);
    905          }
