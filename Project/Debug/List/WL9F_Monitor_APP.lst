    777          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    778          	{
    779          		if(recv_smartkey.Smk_Response_Code == 1)
    780          		{
    781          			switch(recv_smartkey.Smk_Response_Flag)
    782          			{
    783          				case 2 :
    784          					SendSMKMsgResult(SMK_MSG_TAG_NO_REALIZE);
    785          					break;
    786          				case 3 :
    787          					SendSMKMsgResult(SMK_MSG_FAIL);
    788          					break;
    789          				case 4 :
    790          					SendSMKMsgResult(SMK_MSG_TAG_ALREADY_REG);
    791          					break;
    792          				default :
    793          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
    794          					{
    795          						if(recv_smartkey.Registered_Tag_Count == 1)
    796          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
    797          						else if(recv_smartkey.Registered_Tag_Count == 2)
    798          							SendSMKMsgResult(SMK_MSG_TAG_SECOND_REG_SUCCESS);
    799          						else
    800          							SendSMKMsgResult(SMK_MSG_FAIL);
    801          					}
    802          					else
    803          					{
    804          						if(SMK_Tag_Count != 0)
    805          							SMK_Tag_Count--;
    806          
    807          						SendSMKMsgResult(SMK_MSG_FAIL);
    808          					}
    809          					break;
    810          			}
    811          			
    812          		}
    813          		else if(recv_smartkey.Smk_Response_Code == 2)
    814          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
    815                          else
    816                                  SendSMKMsgResult(SMK_MSG_FAIL);
    817          	}
    818          	else
    819          		SendSMKMsgResult(SMK_MSG_FAIL);
    820          }
