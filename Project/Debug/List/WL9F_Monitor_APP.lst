   1169          						AuthResult = 1;
   1170          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
   1171          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
   1172          						SMKSuccess = SMK_SUCCESS;
   1173          						SendSMKAuthResult(SMK_SUCCESS);
   1174          
   1175          						// ++ , 141118 sys3215
   1176          						Flag_ESL=1;
   1177          						// -- , 141118 sys3215
   1178          						
   1179          					}
   1180          				}
   1181          				else
   1182          				{
   1183          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
   1184          					{
   1185          						gAuthentication_Cnt = 0;
   1186          						//AuthResult = 2;
   1187          						AuthResult = 1;
   1188          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
   1189          						SMKSuccess = SMK_FAIL;
   1190          						SendSMKAuthResult(SMK_FAIL);
   1191          						return;
   1192          					}
