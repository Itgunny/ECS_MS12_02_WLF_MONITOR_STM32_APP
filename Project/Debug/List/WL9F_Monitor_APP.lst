    913          void SmartKeyAuthentication(void)
    914          {
    915          	u8  Auth = 0;
    916          	u8 returnvalue;
    917          
    918          	if(AuthResult != 1)
    919          	{
    920          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    921          		{
    922          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    923          			{
    924          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    925          				RequestFirstAuthentication();
    926          			}
    927          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    928          			{
    929          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    930          				RequestSecondAuthentication();
    931          			}
    932          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    933          			{
    934          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    935          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    936          
    937          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    938          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    939          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    940          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    941          					
    942          				returnvalue = CheckResponseAuthentication(Auth);
    943          					
    944          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
    945          				{
    946          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    947          					{
    948          						gAuthentication_Cnt = 0;
    949          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    950          					}
    951          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    952          					{
    953          						gAuthentication_Cnt = 0;
    954          						AuthResult = 1;
    955          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
    956          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
    957          						SMKSuccess = SMK_SUCCESS;
    958          						SendSMKAuthResult(SMK_SUCCESS);
    959          
    960          						// ++ , 141118 sys3215
    961          						Flag_ESL=1;
    962          						// -- , 141118 sys3215
    963          						
    964          					}
    965          				}
    966          				else
    967          				{
    968          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
    969          					{
    970          						//AuthResult = 2;
    971          						AuthResult = 1;
    972          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
    973          						SMKSuccess = SMK_FAIL;
    974          						SendSMKAuthResult(SMK_FAIL);
    975          						return;
    976          					}
    977          
    978          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    979          						smk_flag_data.recv_resp_packet = REQUEST_FIRST_AUTHENTICATION;
    980          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    981          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    982          				}
    983          			}
    984          			else if(smk_flag_data.recv_resp_packet == (REQUEST_FIRST_AUTHENTICATION | RESPONSE_WAIT))
    985          			{
    986          				//RequestFirstAuthentication();
    987          			}
    988          			else if(smk_flag_data.recv_resp_packet == (REQUEST_SECOND_AUTHENTICATION | RESPONSE_WAIT))
    989          			{
    990          				//RequestSecondAuthentication();
    991          			}
    992          		}
    993          	}
    994          	//else if(AuthResult == 1)
    995          	{
    996          		if((Uart2_RxMsg_Smk_Reg_Eli[2] == 1) || (Uart2_RxMsg_Smk_Reg_Eli[2] == 2)) 
