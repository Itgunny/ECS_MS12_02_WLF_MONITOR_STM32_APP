    498          		WL9FM_PowerIG(PowerIG_ON);
    499          		SystemReset = 0;
    500          	}
    501          	else
    502          	{
    503          		if(E2PROM_Save == 0)
    504          		{
    505          			//SaveDataToEEPROM();
    506          
    507          			PwrOffCnt = 0;
    508          			
    509          			while(1)
    510          			{
    511          				//	100msec 마다 15번 체크 -> 1.5초..
    512          				//	PowerIG Off 상태일 경우
    513          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    514          				//	PowerIG On  상태일 경우 
    515          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    516          				
    517          				if(!WL9FM_GetPowerIG())
    518          				{
    519          					
    520          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    521          					
    522          					
    523          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    524          					SystemReset = 0;
    525          					return;
    526          				}
    527          			
    528          				TimeDelay_msec(100);
    529          
    530          				PwrOffCnt++;
    531          				if(PwrOffCnt >= 15)	
    532          					break;
    533          			}
    534          
    535          			Buzzer_Off();
    536          
    537          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    538          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    539          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    540          
    541          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    542          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    543          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    544          
    545          			//	POWER OFF 일 때, LAMP Clear
    546          			LAMP_Update_Data = LAMP_ALL_OFF;
    547          			Old_LAMP_Update_Data = LAMP_ALL_OFF;
    548          		 	Illumination_Sig = 0;
    549          			Lamp_Update_System();
    550          			
    551          			//	엔딩화면 딜레이 시간
    552          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    553          			TimeDelay_msec(1200);
    554          
    555          			//	LCDBL, LCD, LED Off
    556          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    557          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    558          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    559          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    560          
    561          		//	++, kutelf, 140801
    562          		//	RevD.01.01 
    563          		//	Power, PMIC On/Off 방법 변경 
    564          		#if 0
    565          
    566          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    567          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    568          			WL9FM_EXYNOS_PMIC_nRESET();
    569          		#else
    570          			WL9FM_EXYNOS_PMIC_PWROFF();
    571          			
    572          			//	Exynos VDD5V0_4412 Off
    573          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    574          			
    575          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    576          
    577          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    578          			if(!WL9FM_GetPowerIG())
    579          			{
    580          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    581          				SystemReset = 1;			//	Software RESET
    582          
    583          				USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    584          				CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    585          				return;
    586          			}
    587          
    588          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	    
    589          		#endif
    590          		//	--, kutelf, 140801
    591          		}
    592          	}
    593          }
