    497          		WL9FM_PowerIG(PowerIG_ON);
    498          		SystemReset = 0;
    499          	}
    500          	else
    501          	{
    502          		if(E2PROM_Save == 0)
    503          		{
    504          			SaveDataToEEPROM();
    505          
    506          			PwrOffCnt = 0;
    507          
    508          			while(1)
    509          			{
    510          				//	100msec 마다 15번 체크 -> 1.5초..
    511          				//	PowerIG Off 상태일 경우
    512          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    513          				//	PowerIG On  상태일 경우 
    514          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    515          				if(!WL9FM_GetPowerIG())
    516          				{
    517          					//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    518          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    519          					CAN_ITConfig(CAN1, CAN_IT_FMP0, DISABLE);
    520          
    521          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    522          
    523          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    524          					SystemReset = 0;
    525          					return;
    526          				}
    527          			
    528          				TimeDelay_msec(100);
    529          
    530          				PwrOffCnt++;
    531          				if(PwrOffCnt >= 15)	
    532          					break;
    533          			}
    534          
    535          			Buzzer_Off();
    536          
    537          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    538          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    539          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    540          
    541          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    542          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    543          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    544          
    545          			//	POWER OFF 일 때, LAMP Clear
    546          			LAMP_Update_Data = LAMP_ALL_OFF;
    547          			Lamp_Update_System();
    548          			
    549          			//	엔딩화면 딜레이 시간
    550          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    551          			TimeDelay_msec(1200);
    552          
    553          			//	LCDBL, LCD, LED Off
    554          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    555          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    556          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    557          			LED_POWER_ONOFF(LED_OFF);       //  LED Off
    558          
    559          			WL9FM_EXYNOS_PMIC_PWRON();
    560          			
    561          			//	Exynos VDD5V0_4412 Off
    562          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    563          			
    564          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    565          
    566          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    567          			if(!WL9FM_GetPowerIG())
    568          			{
    569          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    570          				SystemReset = 1;			//	Software RESET
    571          				return;
    572          			}
    573          
    574          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	    
    575          		}
    576          	}
    577          }
