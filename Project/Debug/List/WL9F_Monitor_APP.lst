    489          	if(!WL9FM_GetPowerIG())
    490          	{
    491          		WL9FM_PowerIG(PowerIG_ON);
    492          		SystemReset = 0;
    493          	}
    494          	else
    495          	{
    496          		if(E2PROM_Save == 0)
    497          		{
    498          			//SaveDataToEEPROM();
    499          
    500          			PwrOffCnt = 0;
    501          			
    502          			while(1)
    503          			{
    504          				//	100msec 마다 15번 체크 -> 1.5초..
    505          				//	PowerIG Off 상태일 경우
    506          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    507          				//	PowerIG On  상태일 경우 
    508          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    509          				
    510          				if(!WL9FM_GetPowerIG())
    511          				{
    512          					
    513          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    514          					
    515          					
    516          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    517          					SystemReset = 0;
    518          					return;
    519          				}
    520          			
    521          				TimeDelay_msec(100);
    522          
    523          				PwrOffCnt++;
    524          				if(PwrOffCnt >= 15)	
    525          					break;
    526          			}
    527          
    528          			Buzzer_Off();
    529          
    530          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    531          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    532          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    533          
    534          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    535          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    536          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    537          
    538          			//	POWER OFF 일 때, LAMP Clear
    539          			LAMP_Update_Data = LAMP_ALL_OFF;
    540          			Old_LAMP_Update_Data = LAMP_ALL_OFF;
    541          		 	Illumination_Sig = 0;
    542          			Lamp_Update_System();
    543          			
    544          			//	엔딩화면 딜레이 시간
    545          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    546          			TimeDelay_msec(1200);
    547          
    548          			//	LCDBL, LCD, LED Off
    549          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    550          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    551          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    552          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    553          
    554          		//	++, kutelf, 140801
    555          		//	RevD.01.01 
    556          		//	Power, PMIC On/Off 방법 변경 
    557          		#if 0
    558          
    559          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    560          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    561          			WL9FM_EXYNOS_PMIC_nRESET();
    562          		#else
    563          			WL9FM_EXYNOS_PMIC_PWROFF();
    564          			
    565          			//	Exynos VDD5V0_4412 Off
    566          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    567          			
    568          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    569          
    570          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    571          			if(!WL9FM_GetPowerIG())
    572          			{
    573          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    574          				SystemReset = 1;			//	Software RESET
    575          
    576          				USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    577          				CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    578          				return;
    579          			}
    580          
    581          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	    
    582          		#endif
    583          		//	--, kutelf, 140801
    584          		}
    585          	}
    586          }
