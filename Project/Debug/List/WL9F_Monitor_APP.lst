    503          void System_CheckPowerIG()
    504          {
    505          	// Check POWER IG
    506          	if(!WL9FM_GetPowerIG())
    507          	{
    508          		WL9FM_PowerIG(PowerIG_ON);
    509          		SystemReset = 0;
    510          	}
    511          	else
    512          	{
    513          		if(E2PROM_Save == 0)
    514          		{
    515          			//SaveDataToEEPROM();
    516          
    517          			PwrOffCnt = 0;
    518          			
    519          			while(1)
    520          			{
    521          				//	100msec 마다 15번 체크 -> 1.5초..
    522          				//	PowerIG Off 상태일 경우
    523          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    524          				//	PowerIG On  상태일 경우 
    525          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    526          				
    527          				if(!WL9FM_GetPowerIG())
    528          				{
    529          					
    530          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    531          					
    532          					
    533          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    534          					SystemReset = 0;
    535          					return;
    536          				}
    537          			
    538          				TimeDelay_msec(100);
    539          
    540          				PwrOffCnt++;
    541          				if(PwrOffCnt >= 15)	
    542          					break;
    543          			}
    544          
    545          			Buzzer_Off();
    546          
    547          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    548          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(100);
    549          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	//TimeDelay_msec(3);
    550          
    551          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    552          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    553          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    554          
    555          
    556          			Buzzer_UnLimitOff();
    557          			
    558          			//	POWER OFF 일 때, LAMP Clear
    559          			LAMP_Update_Data = LAMP_ALL_OFF;
    560          
    561          			//Old_LAMP_Update_Data = LAMP_ALL_OFF;
    562          
    563          			Illumination_Sig = 0;
    564          			Lamp_Update_System();
    565          			
    566          			//	엔딩화면 딜레이 시간
    567          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    568          			TimeDelay_msec(1200);
    569          
    570          			//	LCDBL, LCD, LED Off
    571          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    572          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    573          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    574          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    575          
    576          		//	++, kutelf, 140801
    577          		//	RevD.01.01 
    578          		//	Power, PMIC On/Off 방법 변경 
    579          		#if 0
    580          
    581          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    582          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    583          			WL9FM_EXYNOS_PMIC_nRESET();
    584          		#else
    585          			WL9FM_EXYNOS_PMIC_PWROFF();
    586          			
    587          			//	Exynos VDD5V0_4412 Off
    588          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    589          			
    590          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    591          
    592          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    593          			if(!WL9FM_GetPowerIG())
    594          			{
    595          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    596          				SystemReset = 1;			//	Software RESET
    597          
    598          				USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    599          				CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    600          				return;
    601          			}
    602          
    603          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	    
    604          		#endif
    605          		//	--, kutelf, 140801
    606          		}
    607          	}
    608          }
