    837          u8 CheckResponseAuthentication(u8 Auth)
    838          {
    839          	u8 output_buf[8];
    840          	u16 upper_cpk, lower_cpk;
    841          
    842          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    843          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    844          	
    845          	setCipherParam(upper_cpk, lower_cpk);
    846          	//setCipherParam(0x1122, 0x3344);	// 임시
    847          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    848          
    849          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    850          	
    851          	
    852          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    853          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    854          	{
    855          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    856          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    857          		{
    858          			return SMK_RESPONSE_FLAG_SUCCESS;
    859          		}
    860          		else
    861          			return recv_smartkey.Smk_Response_Flag;
    862          	}
    863          	else
    864          		return SMK_RESPONSE_CODE_NOT_MATCH;
    865          }
