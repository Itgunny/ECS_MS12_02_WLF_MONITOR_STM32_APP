    589          void System_CheckPowerIG()
    590          {
    591          	// Check POWER IG
    592          	if(!WL9FM_GetPowerIG())
    593          	{
    594          		WL9FM_PowerIG(PowerIG_ON);
    595          		SystemReset = 0;
    596          	}
    597          	else
    598          	{
    599          		if(E2PROM_Save == 0)
    600          		{
    601          			//SaveDataToEEPROM();
    602          
    603          			PwrOffCnt = 0;
    604          			
    605          			while(1)
    606          			{
    607          				//	100msec 마다 15번 체크 -> 1.5초..
    608          				//	PowerIG Off 상태일 경우
    609          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    610          				//	PowerIG On  상태일 경우 
    611          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    612          				
    613          				if(!WL9FM_GetPowerIG())
    614          				{
    615          					
    616          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    617          					
    618          					
    619          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    620          					SystemReset = 0;
    621          					return;
    622          				}
    623          			
    624          				TimeDelay_msec(100);
    625          
    626          				PwrOffCnt++;
    627          				if(PwrOffCnt >= 15)	
    628          					break;
    629          			}
    630          
    631          			// ++, 161021 bwk
    632          			if(SaveSmartKeyUse != SmartKeyUse)
    633          			{
    634          				SaveSMKUseToFlash(SaveSmartKeyUse);
    635          			}
    636          			// --, 161021 bwk
    637          			Buzzer_Off();
    638          
    639          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    640          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(100);
    641          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	//TimeDelay_msec(3);
    642          
    643          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    644          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    645          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    646          
    647          
    648          			Buzzer_UnLimitOff();
    649          			
    650          			//	POWER OFF 일 때, LAMP Clear
    651          			LAMP_Update_Data = LAMP_ALL_OFF;
    652          
    653          			//Old_LAMP_Update_Data = LAMP_ALL_OFF;
    654          
    655          			Illumination_Sig = 0;
    656          			Lamp_Update_System();
    657          			
    658          			//	엔딩화면 딜레이 시간
    659          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    660          			TimeDelay_msec(1200);
    661          
    662          			//	LCDBL, LCD, LED Off
    663          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    664          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    665          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    666          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    667          
    668          		//	++, kutelf, 140801
    669          		//	RevD.01.01 
    670          		//	Power, PMIC On/Off 방법 변경 
    671          		#if 0
    672          
    673          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    674          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    675          			WL9FM_EXYNOS_PMIC_nRESET();
    676          		#else
    677          			WL9FM_EXYNOS_PMIC_PWROFF();
    678          			
    679          			//	Exynos VDD5V0_4412 Off
    680          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    681          			
    682          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    683          
    684          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    685          			if(!WL9FM_GetPowerIG())
    686          			{
    687          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    688          
    689          				//	++, kutelf, 150810
    690          				#if 1
    691          				if (Flag_St_Update==1)
    692          				{
    693          					gRebootCmd=1;
    694          					Flag_St_Update=0;
    695          
    696          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    697          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    698          
    699          					Software_Reset_BootLoader();
    700          				}
    701          				else
    702          				{
    703          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    704          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    705          
    706          					Software_Reset_Application();
