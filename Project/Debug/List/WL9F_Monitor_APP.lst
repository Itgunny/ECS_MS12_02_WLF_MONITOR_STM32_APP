    872          void SmartKeyAuthentication(void)
    873          {
    874          	u8  Auth = 0;
    875          	u8 returnvalue;
    876          
    877          	if(AuthResult != 1)
    878          	{
    879          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    880          		{
    881          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    882          			{
    883          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    884          				RequestFirstAuthentication();
    885          			}
    886          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    887          			{
    888          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    889          				RequestSecondAuthentication();
    890          			}
    891          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    892          			{
    893          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    894          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    895          
    896          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    897          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    898          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    899          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    900          					
    901          				returnvalue = CheckResponseAuthentication(Auth);
    902          					
    903          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
    904          				{
    905          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    906          					{
    907          						gAuthentication_Cnt = 0;
    908          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    909          					}
    910          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    911          					{
    912          						gAuthentication_Cnt = 0;
    913          						AuthResult = 1;
    914          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
    915          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
    916          						SendSMKAuthResult(SMK_SUCCESS);
    917          					}
    918          				}
    919          				else
    920          				{
    921          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
    922          					{
    923          						//AuthResult = 2;
    924          						AuthResult = 1;
    925          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
    926          						SendSMKAuthResult(SMK_FAIL);
    927          						return;
    928          					}
    929          
    930          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    931          						smk_flag_data.recv_resp_packet = REQUEST_FIRST_AUTHENTICATION;
    932          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    933          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    934          				}
    935          			}
    936          			else if(smk_flag_data.recv_resp_packet == (REQUEST_FIRST_AUTHENTICATION | RESPONSE_WAIT))
    937          			{
    938          				//RequestFirstAuthentication();
    939          			}
    940          			else if(smk_flag_data.recv_resp_packet == (REQUEST_SECOND_AUTHENTICATION | RESPONSE_WAIT))
    941          			{
    942          				//RequestSecondAuthentication();
    943          			}
    944          		}
    945          	}
    946          	//else if(AuthResult == 1)
    947          	{
    948          		if((Uart2_RxMsg_Smk_Reg_Eli[0] == 1) || (Uart2_RxMsg_Smk_Reg_Eli[0] == 2)) 
    949          		{
    950          			RequestSMKMessage(Uart2_RxMsg_Smk_Reg_Eli[0]);
    951          			SMK_Msg_Send = 1;
    952          			smk_flag_data.recv_resp_packet = 0;
    953          			Uart2_RxMsg_Smk_Reg_Eli[7] = Uart2_RxMsg_Smk_Reg_Eli[0];
    954          			Uart2_RxMsg_Smk_Reg_Eli[0] = 0xff;
    955          		}
    956          
    957          		if(SMK_Msg_Send == 1)
    958          		{
    959          			if(smk_flag_data.recv_resp_packet == RESPONSE_AUTHENTICATION)
    960          			{
    961          				SMK_Msg_Send = 0;
    962          				CheckResponseMsgComm();
    963          			}
    964          		}
    965          	}
    966          }
    967          #endif
