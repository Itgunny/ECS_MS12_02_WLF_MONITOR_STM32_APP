    998          void SmartKeyAuthentication(void)
    999          {
   1000          	u8  Auth = 0;
   1001          	u8 returnvalue;
   1002          
   1003          	if(AuthResult != 1)
   1004          	{
   1005          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
   1006          		{
   1007          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
   1008          			{
   1009          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
   1010          				RequestFirstAuthentication();
   1011          			}
   1012          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
   1013          			{
   1014          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
   1015          				RequestSecondAuthentication();
   1016          			}
   1017          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
   1018          			{
   1019          
   1020          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
   1021          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
   1022          
   1023          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
   1024          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
   1025          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
   1026          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
   1027          					
   1028          				returnvalue = CheckResponseAuthentication(Auth);
   1029          					
   1030          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
   1031          				{
   1032          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
   1033          					{
