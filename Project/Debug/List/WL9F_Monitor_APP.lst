    822          u8 CheckResponseAuthentication(u8 Auth)
    823          {
    824          	u8 output_buf[8];
    825          	u16 upper_cpk, lower_cpk;
    826          
    827          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    828          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    829          	
    830          	setCipherParam(upper_cpk, lower_cpk);
    831          	//setCipherParam(0x1122, 0x3344);	// 임시
    832          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    833          
    834          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    835          	
    836          	
    837          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    838          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    839          	{
    840          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    841          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    842          		{
    843          			return SMK_RESPONSE_FLAG_SUCCESS;
    844          		}
    845          		else
    846          			return recv_smartkey.Smk_Response_Flag;
    847          	}
    848          	else
    849          		return SMK_RESPONSE_CODE_NOT_MATCH;
    850          }
