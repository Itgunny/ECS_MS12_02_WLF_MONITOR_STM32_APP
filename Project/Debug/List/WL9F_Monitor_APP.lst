    781          u8 CheckResponseAuthentication(u8 Auth)
    782          {
    783          	u8 output_buf[8];
    784          	u16 upper_cpk, lower_cpk;
    785          
    786          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    787          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    788          	
    789          	setCipherParam(upper_cpk, lower_cpk);
    790          	//setCipherParam(0x1122, 0x3344);	// 임시
    791          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    792          
    793          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    794          	
    795          	
    796          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    797          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    798          	{
    799          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    800          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    801          		{
    802          			return SMK_RESPONSE_FLAG_SUCCESS;
    803          		}
    804          		else
    805          			return recv_smartkey.Smk_Response_Flag;
    806          	}
    807          	else
    808          		return SMK_RESPONSE_CODE_NOT_MATCH;
    809          }
