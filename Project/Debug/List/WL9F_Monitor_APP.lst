    857          void SmartKeyAuthentication(void)
    858          {
    859          	u8  Auth = 0;
    860          	u8 returnvalue;
    861          
    862          	if(AuthResult != 1)
    863          	{
    864          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    865          		{
    866          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    867          			{
    868          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    869          				RequestFirstAuthentication();
    870          			}
    871          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    872          			{
    873          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    874          				RequestSecondAuthentication();
    875          			}
    876          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    877          			{
    878          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    879          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    880          
    881          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    882          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    883          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    884          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    885          					
    886          				returnvalue = CheckResponseAuthentication(Auth);
    887          					
    888          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
    889          				{
    890          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    891          					{
    892          						gAuthentication_Cnt = 0;
    893          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    894          					}
    895          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    896          					{
    897          						gAuthentication_Cnt = 0;
    898          						AuthResult = 1;
    899          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
    900          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
    901          						SendSMKAuthResult(SMK_SUCCESS);
    902          					}
    903          				}
    904          				else
    905          				{
    906          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
    907          					{
    908          						//AuthResult = 2;
    909          						AuthResult = 1;
    910          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
    911          						SendSMKAuthResult(SMK_FAIL);
    912          						return;
    913          					}
    914          
    915          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    916          						smk_flag_data.recv_resp_packet = REQUEST_FIRST_AUTHENTICATION;
    917          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    918          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    919          				}
    920          			}
    921          			else if(smk_flag_data.recv_resp_packet == (REQUEST_FIRST_AUTHENTICATION | RESPONSE_WAIT))
    922          			{
    923          				//RequestFirstAuthentication();
    924          			}
    925          			else if(smk_flag_data.recv_resp_packet == (REQUEST_SECOND_AUTHENTICATION | RESPONSE_WAIT))
    926          			{
    927          				//RequestSecondAuthentication();
    928          			}
    929          		}
    930          	}
    931          	//else if(AuthResult == 1)
    932          	{
    933          		if((Uart2_RxMsg_Smk_Reg_Eli[0] == 1) || (Uart2_RxMsg_Smk_Reg_Eli[0] == 2)) 
    934          		{
    935          			RequestSMKMessage(Uart2_RxMsg_Smk_Reg_Eli[0]);
    936          			SMK_Msg_Send = 1;
    937          			smk_flag_data.recv_resp_packet = 0;
    938          			Uart2_RxMsg_Smk_Reg_Eli[7] = Uart2_RxMsg_Smk_Reg_Eli[0];
    939          			Uart2_RxMsg_Smk_Reg_Eli[0] = 0xff;
    940          		}
    941          
    942          		if(SMK_Msg_Send == 1)
    943          		{
    944          			if(smk_flag_data.recv_resp_packet == RESPONSE_AUTHENTICATION)
    945          			{
    946          				SMK_Msg_Send = 0;
    947          				CheckResponseMsgComm();
    948          			}
    949          		}
    950          	}
    951          }
