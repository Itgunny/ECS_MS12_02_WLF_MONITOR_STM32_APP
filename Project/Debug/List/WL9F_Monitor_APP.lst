    542          void System_CheckPowerIG()
    543          {
    544          	// Check POWER IG
    545          	if(!WL9FM_GetPowerIG())
    546          	{
    547          		WL9FM_PowerIG(PowerIG_ON);
    548          		SystemReset = 0;
    549          	}
    550          	else
    551          	{
    552          		if(E2PROM_Save == 0)
    553          		{
    554          			//SaveDataToEEPROM();
    555          
    556          			PwrOffCnt = 0;
    557          			
    558          			while(1)
    559          			{
    560          				//	100msec 마다 15번 체크 -> 1.5초..
    561          				//	PowerIG Off 상태일 경우
    562          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    563          				//	PowerIG On  상태일 경우 
    564          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    565          				
    566          				if(!WL9FM_GetPowerIG())
    567          				{
    568          					
    569          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    570          					
    571          					
    572          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    573          					SystemReset = 0;
    574          					return;
    575          				}
    576          			
    577          				TimeDelay_msec(100);
    578          
    579          				PwrOffCnt++;
    580          				if(PwrOffCnt >= 15)	
    581          					break;
    582          			}
    583          
    584          			Buzzer_Off();
    585          
    586          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    587          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(100);
    588          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	//TimeDelay_msec(3);
    589          
    590          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    591          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    592          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    593          
    594          
    595          			Buzzer_UnLimitOff();
    596          			
    597          			//	POWER OFF 일 때, LAMP Clear
    598          			LAMP_Update_Data = LAMP_ALL_OFF;
    599          
    600          			//Old_LAMP_Update_Data = LAMP_ALL_OFF;
    601          
    602          			Illumination_Sig = 0;
    603          			Lamp_Update_System();
    604          			
    605          			//	엔딩화면 딜레이 시간
    606          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    607          			TimeDelay_msec(1200);
    608          
    609          			//	LCDBL, LCD, LED Off
    610          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    611          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    612          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    613          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    614          
    615          		//	++, kutelf, 140801
    616          		//	RevD.01.01 
    617          		//	Power, PMIC On/Off 방법 변경 
    618          		#if 0
    619          
    620          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    621          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    622          			WL9FM_EXYNOS_PMIC_nRESET();
    623          		#else
    624          			WL9FM_EXYNOS_PMIC_PWROFF();
    625          			
    626          			//	Exynos VDD5V0_4412 Off
    627          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    628          			
    629          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    630          
    631          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    632          			if(!WL9FM_GetPowerIG())
    633          			{
    634          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    635          
    636          				//	++, kutelf, 150810
    637          				#if 1
    638          				if (Flag_St_Update==1)
    639          				{
    640          					gRebootCmd=1;
    641          					Flag_St_Update=0;
    642          
    643          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    644          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    645          
    646          					Software_Reset_BootLoader();
    647          				}
    648          				else
    649          				{
    650          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    651          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    652          
    653          					Software_Reset_Application();
    654          				}	
    655          				#else
    656          				if(Flag_St_Update==1)
    657          				{
    658          					gRebootCmd=1;
    659          					Flag_St_Update=0;
    660          				}
    661          				else
    662          					SystemReset = 1;			//	Software RESET
    663          
    664          				USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    665          				CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    666          				return;
    667          				#endif
    668          				//	--, kutelf, 150810
    669          			}
    670          
    671          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	 
    672          			WL9FM_PowerIG(PowerIG_OFF);		
    673          			WL9FM_PowerIG(PowerIG_OFF);		
    674          		#endif
    675          		//	--, kutelf, 140801
    676          		}
    677          	}
