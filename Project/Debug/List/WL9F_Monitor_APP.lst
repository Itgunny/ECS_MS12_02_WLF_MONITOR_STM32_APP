    497          void System_CheckPowerIG()
    498          {
    499          	// Check POWER IG
    500          	if(!WL9FM_GetPowerIG())
    501          	{
    502          		WL9FM_PowerIG(PowerIG_ON);
    503          		SystemReset = 0;
    504          	}
    505          	else
    506          	{
    507          		if(E2PROM_Save == 0)
    508          		{
    509          			SaveDataToEEPROM();
    510          
    511          			PwrOffCnt = 0;
    512          
    513          			while(1)
    514          			{
    515          				//	100msec 마다 15번 체크 -> 1.5초..
    516          				//	PowerIG Off 상태일 경우
    517          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    518          				//	PowerIG On  상태일 경우 
    519          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    520          				if(!WL9FM_GetPowerIG())
    521          				{
    522          					//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    523          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    524          					CAN_ITConfig(CAN1, CAN_IT_FMP0, DISABLE);
    525          
    526          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    527          
    528          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    529          					SystemReset = 0;
    530          					return;
    531          				}
    532          			
    533          				TimeDelay_msec(100);
    534          
    535          				PwrOffCnt++;
    536          				if(PwrOffCnt >= 15)	
    537          					break;
    538          			}
    539          
    540          			Buzzer_Off();
    541          
    542          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    543          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    544          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(3);
    545          
    546          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    547          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    548          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    549          
    550          			//	POWER OFF 일 때, LAMP Clear
    551          			LAMP_Update_Data = LAMP_ALL_OFF;
    552          			Lamp_Update_System();
    553          			
    554          			//	엔딩화면 딜레이 시간
    555          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    556          			TimeDelay_msec(1200);
    557          
    558          			//	LCDBL, LCD, LED Off
    559          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    560          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    561          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    562          			LED_POWER_ONOFF(LED_OFF);       //  LED Off
    563          
    564          			WL9FM_EXYNOS_PMIC_PWRON();
    565          			
    566          			//	Exynos VDD5V0_4412 Off
    567          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    568          			
    569          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    570          
    571          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    572          			if(!WL9FM_GetPowerIG())
    573          			{
    574          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    575          				SystemReset = 1;			//	Software RESET
    576          				return;
    577          			}
    578          
    579          			WL9FM_PowerIG(PowerIG_OFF);		//  24v Main Power Off	    
    580          		}
    581          	}
    582          }
