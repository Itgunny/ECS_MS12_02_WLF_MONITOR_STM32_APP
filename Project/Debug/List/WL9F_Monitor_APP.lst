    811          void CheckResponseMsgComm(void)
    812          {
    813          	u8 output_buf[8];
    814          	u16 upper_cpk, lower_cpk;
    815          
    816          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    817          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    818          	
    819          	setCipherParam(upper_cpk, lower_cpk);
    820          	//setCipherParam(0x1122, 0x3344);	// 임시
    821          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    822          
    823          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    824          
    825          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    826          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    827          	{
    828          		if(recv_smartkey.Smk_Response_Code == 1)
    829          		{
    830          			switch(recv_smartkey.Smk_Response_Flag)
    831          			{
    832          				case 2 :
    833          					SendSMKMsgResult(SMK_MSG_TAG_NO_REALIZE);
    834          					break;
    835          				case 3 :
    836          					SendSMKMsgResult(SMK_MSG_FAIL);
    837          					break;
    838          				case 4 :
    839          					SendSMKMsgResult(SMK_MSG_TAG_ALREADY_REG);
    840          					break;
    841          				default :
    842          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
    843          					{
    844          						if(recv_smartkey.Registered_Tag_Count == 1)
    845          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
    846          						else if(recv_smartkey.Registered_Tag_Count == 2)
    847          							SendSMKMsgResult(SMK_MSG_TAG_SECOND_REG_SUCCESS);
    848          						else
    849          							SendSMKMsgResult(SMK_MSG_FAIL);
    850          					}
    851          					else
    852          					{
    853          						if(SMK_Tag_Count != 0)
    854          							SMK_Tag_Count--;
    855          
    856          						SendSMKMsgResult(SMK_MSG_FAIL);
    857          					}
    858          					break;
    859          			}
    860          			
    861          		}
    862          		else if(recv_smartkey.Smk_Response_Code == 2)
    863          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
    864                          else
    865                                  SendSMKMsgResult(SMK_MSG_FAIL);
    866          	}
    867          	else
    868          		SendSMKMsgResult(SMK_MSG_FAIL);
    869          }
