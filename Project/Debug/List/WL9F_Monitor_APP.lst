    906          void SmartKeyAuthentication(void)
    907          {
    908          	u8  Auth = 0;
    909          	u8 returnvalue;
    910          
    911          	if(AuthResult != 1)
    912          	{
    913          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    914          		{
    915          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    916          			{
    917          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    918          				RequestFirstAuthentication();
    919          			}
    920          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    921          			{
    922          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    923          				RequestSecondAuthentication();
    924          			}
    925          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    926          			{
    927          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    928          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    929          
    930          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    931          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    932          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    933          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    934          					
    935          				returnvalue = CheckResponseAuthentication(Auth);
    936          					
    937          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
    938          				{
    939          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    940          					{
    941          						gAuthentication_Cnt = 0;
    942          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    943          					}
    944          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    945          					{
    946          						gAuthentication_Cnt = 0;
    947          						AuthResult = 1;
    948          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
    949          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
    950          						SMKSuccess = SMK_SUCCESS;
    951          						SendSMKAuthResult(SMK_SUCCESS);
    952          
    953          						// ++ , 141118 sys3215
    954          						Flag_ESL=1;
    955          						// -- , 141118 sys3215
    956          						
    957          					}
    958          				}
    959          				else
    960          				{
    961          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
    962          					{
    963          						//AuthResult = 2;
    964          						AuthResult = 1;
    965          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
    966          						SMKSuccess = SMK_FAIL;
    967          						SendSMKAuthResult(SMK_FAIL);
    968          						return;
    969          					}
    970          
    971          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    972          						smk_flag_data.recv_resp_packet = REQUEST_FIRST_AUTHENTICATION;
    973          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    974          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    975          				}
    976          			}
    977          			else if(smk_flag_data.recv_resp_packet == (REQUEST_FIRST_AUTHENTICATION | RESPONSE_WAIT))
    978          			{
    979          				//RequestFirstAuthentication();
    980          			}
    981          			else if(smk_flag_data.recv_resp_packet == (REQUEST_SECOND_AUTHENTICATION | RESPONSE_WAIT))
    982          			{
    983          				//RequestSecondAuthentication();
    984          			}
    985          		}
    986          	}
    987          	//else if(AuthResult == 1)
    988          	{
    989          		if((Uart2_RxMsg_Smk_Reg_Eli[2] == 1) || (Uart2_RxMsg_Smk_Reg_Eli[2] == 2)) 
    990          		{
    991          			RequestSMKMessage(Uart2_RxMsg_Smk_Reg_Eli[2]);
    992          			SMK_Msg_Send = 1;
    993          			smk_flag_data.recv_resp_packet = 0;
    994          		//	Uart2_RxMsg_Smk_Reg_Eli[7] = Uart2_RxMsg_Smk_Reg_Eli[0];
    995          			Uart2_RxMsg_Smk_Reg_Eli[2] = 0xff;
    996          		}
    997          
    998          		if(SMK_Msg_Send == 1)
    999          		{
   1000          			if(smk_flag_data.recv_resp_packet == RESPONSE_AUTHENTICATION)
   1001          			{
   1002          				SMK_Msg_Send = 0;
   1003          				CheckResponseMsgComm();
   1004          			}
   1005          		}
   1006          	}
   1007          }
