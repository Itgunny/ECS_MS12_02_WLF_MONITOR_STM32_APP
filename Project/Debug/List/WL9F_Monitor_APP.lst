    896          u8 CheckResponseAuthentication(u8 Auth)
    897          {
    898          	u8 output_buf[8];
    899          	u16 upper_cpk, lower_cpk;
    900          
    901          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    902          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    903          	
    904          	setCipherParam(upper_cpk, lower_cpk);
    905          	//setCipherParam(0x1122, 0x3344);	// 임시
    906          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    907          
    908          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    909          
    910          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    911          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    912          	{
    913          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    914          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    915          		{
    916          			return SMK_RESPONSE_FLAG_SUCCESS;
    917          		}
    918          		else
    919          			return recv_smartkey.Smk_Response_Flag;
    920          	}
    921          	else
    922          		return SMK_RESPONSE_CODE_NOT_MATCH;
    923          }
