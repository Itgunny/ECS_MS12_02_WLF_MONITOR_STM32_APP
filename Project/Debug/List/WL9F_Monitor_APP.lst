   1143          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
   1144          			{
   1145          
   1146          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
   1147          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
