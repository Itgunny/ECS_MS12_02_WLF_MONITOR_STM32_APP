    466          void System_CheckPowerIG()
    467          {
    468          	// Check POWER IG
    469          	if(!WL9FM_GetPowerIG())
    470          	{
    471          		WL9FM_PowerIG(PowerIG_ON);
    472          		SystemReset = 0;
    473          	}
    474          	else
    475          	{
    476          		if(E2PROM_Save == 0)
    477          		{
    478          			SaveDataToEEPROM();
    479          
    480          			PwrOffCnt = 0;
    481          
    482          			while(1)
    483          			{
    484          				if(!WL9FM_GetPowerIG())
    485          				{
    486          					//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    487          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    488          					CAN_ITConfig(CAN1, CAN_IT_FMP0, DISABLE);
    489          
    490          					WL9FM_PowerIG(PowerIG_ON);	// System Reset
    491          					SystemReset = 1;
    492          					return;
    493          				}
    494          			
    495          				TimeDelay_msec(100);
    496          
    497          				PwrOffCnt++;
    498          				if(PwrOffCnt >= 15)	//	100msec 마다 15번 체크 -> 1.5초..
    499          					break;
    500          			}
    501          
    502          			Buzzer_Off();
    503          
    504          			//	POWER OFF Code를 3msec 마다 2번 SPICA로 보낸다. 
    505          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	
    506          			TimeDelay_msec(3);
    507          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	
    508          			TimeDelay_msec(3);
    509          
    510          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    511          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    512          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    513          
    514          			//	엔딩화면 딜레이 시간
    515          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    516          			TimeDelay_msec(1200);
    517          
    518          			LCDBL_PWM_LEVEL(0);                         	//  LCDBL PWM LEVEL0
    519          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On!!!
    520          	
    521          	        
    522          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    523          			if(!WL9FM_GetPowerIG())
    524          			{
    525          				WL9FM_PowerIG(PowerIG_ON);
    526          				SystemReset = 1;
    527          				return;
    528          			}
    529          	        
    530          			LCD_POWER_ONOFF(LCDPWR_OFF);                        //  LCD Power Off
    531          			LED_POWER_ONOFF(LED_OFF);                        	//  LED Off
    532          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);	
    533          			WL9FM_PowerIG(PowerIG_OFF);                    //  24v Main Power Off	        
    534          		}
    535          	}
    536          }
