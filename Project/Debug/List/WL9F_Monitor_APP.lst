    870          u8 CheckResponseAuthentication(u8 Auth)
    871          {
    872          	u8 output_buf[8];
    873          	u16 upper_cpk, lower_cpk;
    874          
    875          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    876          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    877          	
    878          	setCipherParam(upper_cpk, lower_cpk);
    879          	//setCipherParam(0x1122, 0x3344);	// 임시
    880          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    881          
    882          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    883          
    884          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    885          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    886          	{
    887          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    888          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    889          		{
    890          			return SMK_RESPONSE_FLAG_SUCCESS;
    891          		}
    892          		else
    893          			return recv_smartkey.Smk_Response_Flag;
    894          	}
    895          	else
    896          		return SMK_RESPONSE_CODE_NOT_MATCH;
    897          }
