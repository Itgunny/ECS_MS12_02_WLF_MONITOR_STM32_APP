    971          void SmartKeyAuthentication(void)
    972          {
    973          	u8  Auth = 0;
    974          	u8 returnvalue;
    975          
    976          	if(AuthResult != 1)
    977          	{
    978          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    979          		{
    980          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    981          			{
    982          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    983          				RequestFirstAuthentication();
    984          			}
    985          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    986          			{
    987          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    988          				RequestSecondAuthentication();
    989          			}
    990          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    991          			{
    992          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    993          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    994          
    995          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    996          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    997          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    998          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    999          					
   1000          				returnvalue = CheckResponseAuthentication(Auth);
   1001          					
   1002          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
   1003          				{
   1004          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
   1005          					{
   1006          						gAuthentication_Cnt = 0;
   1007          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
   1008          					}
   1009          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
   1010          					{
   1011          						gAuthentication_Cnt = 0;
   1012          						AuthResult = 1;
   1013          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
   1014          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
   1015          						SMKSuccess = SMK_SUCCESS;
   1016          						SendSMKAuthResult(SMK_SUCCESS);
   1017          
   1018          						// ++ , 141118 sys3215
   1019          						Flag_ESL=1;
   1020          						// -- , 141118 sys3215
   1021          						
   1022          					}
