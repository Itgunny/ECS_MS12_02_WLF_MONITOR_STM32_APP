    845          void SmartKeyAuthentication(void)
    846          {
    847          	u8  Auth = 0;
    848          	u8 returnvalue;
    849          
    850          	if(AuthResult != 1)
    851          	{
    852          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    853          		{
    854          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    855          			{
    856          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    857          				RequestFirstAuthentication();
    858          			}
    859          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    860          			{
    861          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    862          				RequestSecondAuthentication();
    863          			}
    864          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    865          			{
    866          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    867          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    868          
    869          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    870          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    871          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    872          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    873          					
    874          				returnvalue = CheckResponseAuthentication(Auth);
    875          					
    876          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
    877          				{
    878          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    879          					{
    880          						gAuthentication_Cnt = 0;
    881          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    882          					}
    883          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    884          					{
    885          						gAuthentication_Cnt = 0;
    886          						AuthResult = 1;
    887          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
    888          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
    889          						SendSMKAuthResult(SMK_SUCCESS);
    890          					}
    891          				}
    892          				else
    893          				{
    894          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
    895          					{
    896          						//AuthResult = 2;
    897          						AuthResult = 1;
    898          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
    899          						SendSMKAuthResult(SMK_FAIL);
    900          						return;
    901          					}
    902          
    903          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    904          						smk_flag_data.recv_resp_packet = REQUEST_FIRST_AUTHENTICATION;
    905          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    906          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    907          				}
    908          			}
    909          		}
    910          	}
    911          	//else if(AuthResult == 1)
    912          	{
    913          		if((Uart2_RxMsg_Smk_Reg_Eli[0] == 1) || (Uart2_RxMsg_Smk_Reg_Eli[0] == 2)) 
    914          		{
    915          			RequestSMKMessage(Uart2_RxMsg_Smk_Reg_Eli[0]);
    916          			SMK_Msg_Send = 1;
    917          			smk_flag_data.recv_resp_packet = 0;
    918          			Uart2_RxMsg_Smk_Reg_Eli[7] = Uart2_RxMsg_Smk_Reg_Eli[0];
    919          			Uart2_RxMsg_Smk_Reg_Eli[0] = 0xff;
    920          		}
    921          
    922          		if(SMK_Msg_Send == 1)
    923          		{
    924          			if(smk_flag_data.recv_resp_packet == RESPONSE_AUTHENTICATION)
    925          			{
    926          				SMK_Msg_Send = 0;
    927          				CheckResponseMsgComm();
    928          			}
    929          		}
    930          	}
    931          }
