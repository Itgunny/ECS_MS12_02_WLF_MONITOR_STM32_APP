    935          void SmartKeyAuthentication(void)
    936          {
    937          	u8  Auth = 0;
    938          	u8 returnvalue;
    939          
    940          	if(AuthResult != 1)
    941          	{
    942          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    943          		{
    944          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    945          			{
    946          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    947          				RequestFirstAuthentication();
    948          			}
    949          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    950          			{
    951          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    952          				RequestSecondAuthentication();
    953          			}
    954          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    955          			{
    956          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    957          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    958          
    959          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    960          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    961          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    962          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    963          					
    964          				returnvalue = CheckResponseAuthentication(Auth);
    965          					
    966          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
    967          				{
    968          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    969          					{
    970          						gAuthentication_Cnt = 0;
    971          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    972          					}
    973          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    974          					{
    975          						gAuthentication_Cnt = 0;
    976          						AuthResult = 1;
    977          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
    978          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
    979          						SMKSuccess = SMK_SUCCESS;
    980          						SendSMKAuthResult(SMK_SUCCESS);
    981          
    982          						// ++ , 141118 sys3215
    983          						Flag_ESL=1;
    984          						// -- , 141118 sys3215
    985          						
    986          					}
    987          				}
    988          				else
    989          				{
    990          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
    991          					{
    992          						//AuthResult = 2;
    993          						AuthResult = 1;
    994          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
    995          						SMKSuccess = SMK_FAIL;
    996          						SendSMKAuthResult(SMK_FAIL);
    997          						return;
    998          					}
    999          
   1000          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
   1001          						smk_flag_data.recv_resp_packet = REQUEST_FIRST_AUTHENTICATION;
   1002          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
   1003          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
   1004          				}
   1005          			}
   1006          			else if(smk_flag_data.recv_resp_packet == (REQUEST_FIRST_AUTHENTICATION | RESPONSE_WAIT))
   1007          			{
   1008          				//RequestFirstAuthentication();
   1009          			}
   1010          			else if(smk_flag_data.recv_resp_packet == (REQUEST_SECOND_AUTHENTICATION | RESPONSE_WAIT))
   1011          			{
   1012          				//RequestSecondAuthentication();
   1013          			}
