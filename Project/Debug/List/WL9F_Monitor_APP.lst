    822          void SmartKeyAuthentication(void)
    823          {
    824          	u8  Auth = 0;
    825          	u8 returnvalue;
    826          
    827          	if(AuthResult != 1)
    828          	{
    829          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    830          		{
    831          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    832          			{
    833          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    834          				RequestFirstAuthentication();
    835          			}
    836          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    837          			{
    838          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    839          				RequestSecondAuthentication();
    840          			}
    841          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    842          			{
    843          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    844          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    845          
    846          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    847          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    848          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    849          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    850          					
    851          				returnvalue = CheckResponseAuthentication(Auth);
    852          					
    853          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
    854          				{
    855          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    856          					{
    857          						gAuthentication_Cnt = 0;
    858          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    859          					}
    860          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    861          					{
    862          						gAuthentication_Cnt = 0;
    863          						AuthResult = 1;
    864          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
    865          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
    866          						SendSMKAuthResult(SMK_SUCCESS);
    867          					}
    868          				}
    869          				else
    870          				{
    871          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
    872          					{
    873          						//AuthResult = 2;
    874          						AuthResult = 1;
    875          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
    876          						SendSMKAuthResult(SMK_FAIL);
    877          						return;
    878          					}
    879          
    880          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    881          						smk_flag_data.recv_resp_packet = REQUEST_FIRST_AUTHENTICATION;
    882          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    883          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    884          				}
    885          			}
    886          		}
    887          	}
    888          	//else if(AuthResult == 1)
    889          	{
    890          		if((Uart2_RxMsg_Smk_Reg_Eli[0] == 1) || (Uart2_RxMsg_Smk_Reg_Eli[0] == 2)) 
    891          		{
    892          			RequestSMKMessage(Uart2_RxMsg_Smk_Reg_Eli[0]);
    893          			SMK_Msg_Send = 1;
    894          			smk_flag_data.recv_resp_packet = 0;
    895          			Uart2_RxMsg_Smk_Reg_Eli[7] = Uart2_RxMsg_Smk_Reg_Eli[0];
    896          			Uart2_RxMsg_Smk_Reg_Eli[0] = 0xff;
    897          		}
    898          
    899          		if(SMK_Msg_Send == 1)
    900          		{
    901          			if(smk_flag_data.recv_resp_packet == RESPONSE_AUTHENTICATION)
    902          			{
    903          				SMK_Msg_Send = 0;
    904          				CheckResponseMsgComm();
    905          			}
    906          		}
    907          	}
    908          }
