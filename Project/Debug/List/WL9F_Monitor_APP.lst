    463          void System_CheckPowerIG()
    464          {
    465          	// Check POWER IG
    466          	if(!WL9FM_GetPowerIG())
    467          	{
    468          		WL9FM_PowerIG(PowerIG_ON);
    469          		SystemReset = 0;
    470          	}
    471          	else
    472          	{
    473          		if(E2PROM_Save == 0)
    474          		{
    475          			SaveDataToEEPROM();
    476          
    477          			PwrOffCnt = 0;
    478          
    479          			while(1)
    480          			{
    481          				if(!WL9FM_GetPowerIG())
    482          				{
    483          					//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    484          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    485          					CAN_ITConfig(CAN1, CAN_IT_FMP0, DISABLE);
    486          
    487          					WL9FM_PowerIG(PowerIG_ON);	// System Reset
    488          					SystemReset = 1;
    489          					return;
    490          				}
    491          			
    492          				TimeDelay_msec(100);
    493          
    494          				PwrOffCnt++;
    495          				if(PwrOffCnt >= 15)	//	100msec 마다 15번 체크 -> 1.5초..
    496          					break;
    497          			}
    498          
    499          			Buzzer_Off();
    500          
    501          			//	POWER OFF Code를 3msec 마다 2번 SPICA로 보낸다. 
    502          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	
    503          			TimeDelay_msec(3);
    504          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	
    505          			TimeDelay_msec(3);
    506          
    507          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    508          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    509          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    510          
    511          			//	엔딩화면 딜레이 시간
    512          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    513          			TimeDelay_msec(1200);
    514          
    515          			LCDBL_PWM_LEVEL(0);                         	//  LCDBL PWM LEVEL0
    516          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On!!!
    517          	
    518          	        
    519          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    520          			if(!WL9FM_GetPowerIG())
    521          			{
    522          				WL9FM_PowerIG(PowerIG_ON);
    523          				SystemReset = 1;
    524          				return;
    525          			}
    526          	        
    527          			LCD_POWER_ONOFF(LCDPWR_OFF);                        //  LCD Power Off
    528          			LED_POWER_ONOFF(LED_OFF);                        	//  LED Off
    529          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);	
    530          			WL9FM_PowerIG(PowerIG_OFF);                    //  24v Main Power Off	        
    531          		}
    532          	}
    533          }
