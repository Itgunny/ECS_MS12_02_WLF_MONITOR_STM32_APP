    815          u8 CheckResponseAuthentication(u8 Auth)
    816          {
    817          	u8 output_buf[8];
    818          	u16 upper_cpk, lower_cpk;
    819          
    820          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    821          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    822          	
    823          	setCipherParam(upper_cpk, lower_cpk);
    824          	//setCipherParam(0x1122, 0x3344);	// 임시
    825          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    826          
    827          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    828          	
    829          	
    830          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    831          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    832          	{
    833          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    834          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    835          		{
    836          			return SMK_RESPONSE_FLAG_SUCCESS;
    837          		}
    838          		else
    839          			return recv_smartkey.Smk_Response_Flag;
    840          	}
    841          	else
    842          		return SMK_RESPONSE_CODE_NOT_MATCH;
    843          }
    844          
    845          void CheckResponseMsgComm(void)
    846          {
    847          	u8 output_buf[8];
    848          	u16 upper_cpk, lower_cpk;
    849          
    850          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    851          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    852          	
    853          	setCipherParam(upper_cpk, lower_cpk);
    854          	//setCipherParam(0x1122, 0x3344);	// 임시
    855          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    856          
    857          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    858          
    859          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    860          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    861          	{
    862          		if(recv_smartkey.Smk_Response_Code == 1)
    863          		{
    864          			switch(recv_smartkey.Smk_Response_Flag)
    865          			{
    866          				case 2 :
    867          					SendSMKMsgResult(SMK_MSG_TAG_NO_REALIZE);
    868          					break;
    869          				case 3 :
    870          					SendSMKMsgResult(SMK_MSG_FAIL);
    871          					break;
    872          				case 4 :
    873          					SendSMKMsgResult(SMK_MSG_TAG_ALREADY_REG);
    874          					break;
    875          				default :
    876          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
    877          					{
    878          						if(recv_smartkey.Registered_Tag_Count == 1)
    879          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
    880          						else if(recv_smartkey.Registered_Tag_Count == 2)
    881          							SendSMKMsgResult(SMK_MSG_TAG_SECOND_REG_SUCCESS);
    882          						else
    883          							SendSMKMsgResult(SMK_MSG_FAIL);
    884          					}
    885          					else
    886          					{
    887          						if(SMK_Tag_Count != 0)
    888          							SMK_Tag_Count--;
    889          
    890          						SendSMKMsgResult(SMK_MSG_FAIL);
    891          					}
    892          					break;
    893          			}
    894          			
    895          		}
    896          		else if(recv_smartkey.Smk_Response_Code == 2)
    897          			SendSMKMsgResult(SMK_MSG_TAG_ELIMINATION_SUCCESS);
    898          		else
    899          			SendSMKMsgResult(SMK_MSG_FAIL);
    900          	}
    901          	else
    902          		SendSMKMsgResult(SMK_MSG_FAIL);
    903          }
