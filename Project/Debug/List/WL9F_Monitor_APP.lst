    847          void CheckResponseMsgComm(void)
    848          {
    849          	u8 output_buf[8];
    850          	u16 upper_cpk, lower_cpk;
    851          
    852          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    853          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    854          	
    855          	setCipherParam(upper_cpk, lower_cpk);
    856          	//setCipherParam(0x1122, 0x3344);	// 임시
    857          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    858          
    859          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    860          
    861          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    862          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    863          	{
    864          		if(recv_smartkey.Smk_Response_Code == 1)
    865          		{
    866          			switch(recv_smartkey.Smk_Response_Flag)
    867          			{
    868          				case 2 :
    869          					SendSMKMsgResult(SMK_MSG_TAG_NO_REALIZE);
    870          					break;
    871          				case 3 :
