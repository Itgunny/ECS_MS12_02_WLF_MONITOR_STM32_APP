    780          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    781          	{
    782          		if(recv_smartkey.Smk_Response_Code == 1)
    783          		{
    784          			switch(recv_smartkey.Smk_Response_Flag)
    785          			{
    786          				case 2 :
