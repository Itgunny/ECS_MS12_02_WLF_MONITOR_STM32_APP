    825          void SmartKeyAuthentication(void)
    826          {
    827          	u8  Auth = 0;
    828          	u8 returnvalue;
    829          
    830          	if(AuthResult != 1)
    831          	{
    832          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
    833          		{
    834          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
    835          			{
    836          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    837          				RequestFirstAuthentication();
    838          			}
    839          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
    840          			{
    841          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
    842          				RequestSecondAuthentication();
    843          			}
    844          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
    845          			{
    846          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
    847          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
    848          
    849          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    850          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
    851          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    852          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
    853          					
    854          				returnvalue = CheckResponseAuthentication(Auth);
    855          					
    856          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
    857          				{
    858          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    859          					{
    860          						gAuthentication_Cnt = 0;
    861          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    862          					}
    863          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    864          					{
    865          						gAuthentication_Cnt = 0;
    866          						AuthResult = 1;
    867          						SMK_Tag_Count = recv_smartkey.Registered_Tag_Count;
    868          						smk_flag_data.recv_resp_packet = RESPONSE_SUCCESS;
    869          						SendSMKAuthResult(SMK_SUCCESS);
    870          					}
    871          				}
    872          				else
    873          				{
    874          					if(++gAuthentication_Cnt >= TIME_OUT_COUNT)
    875          					{
    876          						//AuthResult = 2;
    877          						AuthResult = 1;
    878          						smk_flag_data.recv_resp_packet = RESPONSE_TIME_OUT;
    879          						SendSMKAuthResult(SMK_FAIL);
    880          						return;
    881          					}
    882          
    883          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
    884          						smk_flag_data.recv_resp_packet = REQUEST_FIRST_AUTHENTICATION;
    885          					else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
    886          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
    887          				}
    888          			}
    889          		}
    890          	}
    891          	//else if(AuthResult == 1)
    892          	{
    893          		if((Uart2_RxMsg_Smk_Reg_Eli[0] == 1) || (Uart2_RxMsg_Smk_Reg_Eli[0] == 2)) 
    894          		{
    895          			RequestSMKMessage(Uart2_RxMsg_Smk_Reg_Eli[0]);
    896          			SMK_Msg_Send = 1;
    897          			smk_flag_data.recv_resp_packet = 0;
    898          			Uart2_RxMsg_Smk_Reg_Eli[7] = Uart2_RxMsg_Smk_Reg_Eli[0];
    899          			Uart2_RxMsg_Smk_Reg_Eli[0] = 0xff;
    900          		}
    901          
    902          		if(SMK_Msg_Send == 1)
    903          		{
    904          			if(smk_flag_data.recv_resp_packet == RESPONSE_AUTHENTICATION)
    905          			{
    906          				SMK_Msg_Send = 0;
    907          				CheckResponseMsgComm();
    908          			}
    909          		}
    910          	}
    911          }
