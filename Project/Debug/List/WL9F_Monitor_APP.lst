    732          u8 CheckResponseAuthentication(u8 Auth)
    733          {
    734          	u8 output_buf[8];
    735          	u16 upper_cpk, lower_cpk;
    736          
    737          	upper_cpk = (rand_value.rand_cpk & 0xffff0000) >> 16;
    738          	lower_cpk = rand_value.rand_cpk & 0x0000ffff;
    739          	
    740          	setCipherParam(upper_cpk, lower_cpk);
    741          	//setCipherParam(0x1122, 0x3344);	// 임시
    742          	decode((u8*)&recv_smartkey, 8, (u8*)&output_buf[0], 8);
    743          
    744          	memcpy((u8*)&recv_smartkey, (u8*)&output_buf[0], 8);
    745          	
    746          	
    747          	if((send_smartkey.TagLevel == recv_smartkey.ReturnTagLevel) && (send_smartkey.TagCommand == recv_smartkey.ReturnTagCommand) 
    748          		&& (send_smartkey.VMC == recv_smartkey.ReturnVMC))
    749          	{
    750          		if((recv_smartkey.Smk_Response_Code == Auth) && 
    751          			(recv_smartkey.Smk_Response_Flag == SMK_RESPONSE_FLAG_SUCCESS))
    752          		{
    753          			return SMK_RESPONSE_FLAG_SUCCESS;
    754          		}
    755          		else
    756          			return recv_smartkey.Smk_Response_Flag;
    757          	}
    758          	else
    759          		return SMK_RESPONSE_CODE_NOT_MATCH;
    760          }
