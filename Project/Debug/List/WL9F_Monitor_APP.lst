    873          					break;
    874          				case 4 :
    875          					SendSMKMsgResult(SMK_MSG_TAG_ALREADY_REG);
    876          					break;
    877          				default :
    878          					if(recv_smartkey.ReturnTagCommand == SMK_RESPONSE_CODE_REGISTRATION)
    879          					{
    880          						if(recv_smartkey.Registered_Tag_Count == 1)
    881          							SendSMKMsgResult(SMK_MSG_TAG_FIRST_REG_SUCCESS);
    882          						else if(recv_smartkey.Registered_Tag_Count == 2)
