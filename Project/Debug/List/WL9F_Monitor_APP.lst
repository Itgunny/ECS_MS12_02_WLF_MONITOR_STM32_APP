    590          void System_CheckPowerIG()
    591          {
    592          	// Check POWER IG
    593          	if(!WL9FM_GetPowerIG())
    594          	{
    595          		WL9FM_PowerIG(PowerIG_ON);
    596          		SystemReset = 0;
    597          	}
    598          	else
    599          	{
    600          		if(E2PROM_Save == 0)
    601          		{
    602          			//SaveDataToEEPROM();
    603          
    604          			PwrOffCnt = 0;
    605          			
    606          			while(1)
    607          			{
    608          				//	100msec 마다 15번 체크 -> 1.5초..
    609          				//	PowerIG Off 상태일 경우
    610          				//		-> while 루프를 나와서 PowerOff Logic 진행  
    611          				//	PowerIG On  상태일 경우 
    612          				//		-> PowerIG Control 신호를 Enable하고, 정상 동작으로 진행 
    613          				
    614          				if(!WL9FM_GetPowerIG())
    615          				{
    616          					
    617          					WL9FM_PowerIG(PowerIG_ON);	// PowerIG Set
    618          					
    619          					
    620          					//	1.5초 동안 체크해서 PowerIG가 다시 들어오면, Software Reset을 하지 않고, 다시 동작
    621          					SystemReset = 0;
    622          					return;
    623          				}
    624          			
    625          				TimeDelay_msec(100);
    626          
    627          				PwrOffCnt++;
    628          				if(PwrOffCnt >= 15)	
    629          					break;
    630          			}
    631          
    632          			// ++, 161021 bwk
    633          			if(SaveSmartKeyUse != SmartKeyUse)
    634          			{
    635          				SaveSMKUseToFlash(SaveSmartKeyUse);
    636          			}
    637          			// --, 161021 bwk
    638          			Buzzer_Off();
    639          
    640          			//	POWER OFF Code를 3msec 마다 2번 Exynos로 보낸다. 
    641          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	TimeDelay_msec(100);
    642          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	//TimeDelay_msec(3);
    643          
    644          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    645          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    646          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    647          
    648          
    649          			Buzzer_UnLimitOff();
    650          			
    651          			//	POWER OFF 일 때, LAMP Clear
    652          			LAMP_Update_Data = LAMP_ALL_OFF;
    653          
    654          			//Old_LAMP_Update_Data = LAMP_ALL_OFF;
    655          
    656          			Illumination_Sig = 0;
    657          			Lamp_Update_System();
    658          			
    659          			//	엔딩화면 딜레이 시간
    660          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    661          			TimeDelay_msec(1200);
    662          
    663          			//	LCDBL, LCD, LED Off
    664          			LCDBL_PWM_LEVEL(0);            	//  LCDBL PWM LEVEL0
    665          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On
    666          			LCD_POWER_ONOFF(LCDPWR_OFF);	//  LCD Power Off
    667          			//LED_POWER_ONOFF(LED_OFF);       //  LED Off
    668          
    669          		//	++, kutelf, 140801
    670          		//	RevD.01.01 
    671          		//	Power, PMIC On/Off 방법 변경 
    672          		#if 0
    673          
    674          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_PMIC_OFF);	//	->	GPIO_Control.c EXYNOS-4412 Power On..
    675          			WL9FM_EXYNOS_PMIC_PWRONOFF(EXYNOS_PMIC_OFF);
    676          			WL9FM_EXYNOS_PMIC_nRESET();
    677          		#else
    678          			WL9FM_EXYNOS_PMIC_PWROFF();
    679          			
    680          			//	Exynos VDD5V0_4412 Off
    681          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    682          			
    683          			TimeDelay_msec(1000);			//	1.5초 동안. 대기한 후, 마지막으로 PowerIG를 체크 
    684          
    685          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    686          			if(!WL9FM_GetPowerIG())
    687          			{
    688          				WL9FM_PowerIG(PowerIG_ON);	// 	PowerIG Set
    689          
    690          				//	++, kutelf, 150810
    691          				#if 1
    692          				if (Flag_St_Update==1)
    693          				{
    694          					gRebootCmd=1;
    695          					Flag_St_Update=0;
    696          
    697          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    698          					CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    699          
