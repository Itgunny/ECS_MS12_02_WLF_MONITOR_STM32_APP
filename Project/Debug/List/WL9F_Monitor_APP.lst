   1125          	u8  Auth = 0;
   1126          	u8 returnvalue;
   1127          
   1128          	if(AuthResult != 1)
   1129          	{
   1130          		if((smk_flag_data.recv_resp_packet != RESPONSE_SUCCESS) && (smk_flag_data.recv_resp_packet != RESPONSE_TIME_OUT))
   1131          		{
   1132          			if(smk_flag_data.recv_resp_packet == REQUEST_FIRST_AUTHENTICATION)
   1133          			{
   1134          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
   1135          				RequestFirstAuthentication();
   1136          			}
   1137          			else if(smk_flag_data.recv_resp_packet == REQUEST_SECOND_AUTHENTICATION)
   1138          			{
   1139          				smk_flag_data.recv_resp_packet |= RESPONSE_WAIT;
   1140          				RequestSecondAuthentication();
   1141          			}
   1142          			else if(smk_flag_data.recv_resp_packet & RESPONSE_AUTHENTICATION)
   1143          			{
   1144          
   1145          				//smk_flag_data.recv_resp_packet &= (~(RESPONSE_AUTHENTICATION));
   1146          				smk_flag_data.recv_resp_packet = (smk_flag_data.recv_resp_packet & (~(RESPONSE_AUTHENTICATION)));
   1147          
   1148          				if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
   1149          					Auth = SMK_RESPOSEN_CODE_COMM_AUTHENTICATION;
   1150          				else if(smk_flag_data.recv_resp_packet & REQUEST_SECOND_AUTHENTICATION)
   1151          					Auth = SMK_RESPONSE_CODE_AUTHENTICATION;
   1152          					
   1153          				returnvalue = CheckResponseAuthentication(Auth);
   1154          					
   1155          				if(returnvalue == SMK_RESPONSE_FLAG_SUCCESS)	// Success
   1156          				{
   1157          					if(smk_flag_data.recv_resp_packet & REQUEST_FIRST_AUTHENTICATION)
   1158          					{
   1159          						gAuthentication_Cnt = 0;
   1160          						smk_flag_data.recv_resp_packet = REQUEST_SECOND_AUTHENTICATION;
   1161                                                          // ++, 150713 bwk
