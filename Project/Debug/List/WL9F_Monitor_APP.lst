    476          void System_CheckPowerIG()
    477          {
    478          	// Check POWER IG
    479          	if(!WL9FM_GetPowerIG())
    480          	{
    481          		WL9FM_PowerIG(PowerIG_ON);
    482          		SystemReset = 0;
    483          	}
    484          	else
    485          	{
    486          		if(E2PROM_Save == 0)
    487          		{
    488          			SaveDataToEEPROM();
    489          
    490          			PwrOffCnt = 0;
    491          
    492          			while(1)
    493          			{
    494          				if(!WL9FM_GetPowerIG())
    495          				{
    496          					//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    497          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    498          					CAN_ITConfig(CAN1, CAN_IT_FMP0, DISABLE);
    499          
    500          					WL9FM_PowerIG(PowerIG_ON);	// System Reset
    501          					//SystemReset = 1;
    502          					SystemReset = 0;
    503          					return;
    504          				}
    505          			
    506          				TimeDelay_msec(100);
    507          
    508          				PwrOffCnt++;
    509          				if(PwrOffCnt >= 15)	//	100msec 마다 15번 체크 -> 1.5초..
    510          					break;
    511          			}
    512          
    513          			Buzzer_Off();
    514          
    515          			//	POWER OFF Code를 3msec 마다 2번 SPICA로 보낸다. 
    516          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	
    517          			TimeDelay_msec(3);
    518          			KeySwitch_SendToEXYNOS(KEYSWITCH_POWER_OFF,0);	
    519          			TimeDelay_msec(3);
    520          
    521          			//	POWER OFF 일 때, 시리얼과 캔 통신 부분을 죽여놔야지, 정상적으로 RESET 동작을 수행
    522          			USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    523          			CAN_ITConfig(CAN1,CAN_IT_FMP0, DISABLE);
    524          
    525          			//	엔딩화면 딜레이 시간
    526          			//	1200msec 이상 설정하면, LCD가 하얗게 된다. -> POWER Off Time으로 인하여
    527          
    528          			LAMP_Update_Data = LAMP_ALL_OFF;
    529          			Lamp_Update_System();
    530          			
    531          			TimeDelay_msec(1200);
    532          
    533          			LCDBL_PWM_LEVEL(0);                         	//  LCDBL PWM LEVEL0
    534          			LCDBL_ONOFF(LCDBL_OFF);			//  LCDBL Power On!!!
    535          	
    536          
    537          			LCD_POWER_ONOFF(LCDPWR_OFF);                        //  LCD Power Off
    538          			LED_POWER_ONOFF(LED_OFF);                        	//  LED Off
    539          
    540          			
    541          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    542          			TimeDelay_msec(100);
    543          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_ON);
    544          			
    545          			WL9FM_EXYNOS_PMIC_ONOFF();
    546          				
    547          			
    548          			
    549          			
    550          			WL9FM_EXYNOS_POWER_ONOFF(EXYNOS_POWER_OFF);
    551          			
    552          			
    553          			//	엔딩화면이 지난 후에 PowerIG가 다시 들어오면, System을 RESET 시킨다. 
    554          			TimeDelay_msec(1000);
    555          			if(!WL9FM_GetPowerIG())
    556          			{
    557          				WL9FM_PowerIG(PowerIG_ON);
    558          				SystemReset = 1;
    559          				return;
    560          			}
    561          
    562          			WL9FM_PowerIG(PowerIG_OFF);                    //  24v Main Power Off	    
    563          
    564          			
    565          		}
    566          	}
    567          }
