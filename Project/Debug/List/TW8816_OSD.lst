    418          void SetDisplayWarning(void)
    419          {
    420            u8 i;
    421            WriteTW8816(0x94, 0x02);
    422            for(i = 0; i < 27; i++){
    423              WriteTW8816(0x94, 0x80);
    424              WriteTW8816(0x95, 0x81);
    425              WriteTW8816(0x96, (i + 0x9b));
    426              WriteTW8816(0x97, (i + 0x7B));
    427              WriteTW8816(0x98, 0x06);    
    428            }
    429          }
