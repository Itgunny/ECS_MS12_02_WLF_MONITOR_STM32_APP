    377          unsigned short MakeCrc16(unsigned char *data, unsigned short nLen)
    378          {
    379              unsigned short i = 0;
    380              unsigned short crc16 = 0;
    381              
    382              while (nLen--)
    383              {
    384                  crc16 = Crc16Table[((crc16 ^ data[i]) & 0xff)] ^ (crc16 >> 8);
    385                  i++;
    386              }
    387              
    388              return(crc16);
    389          }
