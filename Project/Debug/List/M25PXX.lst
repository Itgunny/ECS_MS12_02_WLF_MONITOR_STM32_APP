    379          unsigned short MakeCrc16(unsigned char *data, unsigned short nLen)
    380          {
    381              unsigned short i = 0;
    382              unsigned short crc16 = 0;
    383              
    384              while (nLen--)
    385              {
    386                  crc16 = Crc16Table[((crc16 ^ data[i]) & 0xff)] ^ (crc16 >> 8);
    387                  i++;
    388              }
    389              
    390              return(crc16);
    391          }
