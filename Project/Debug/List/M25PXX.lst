    316          unsigned char SPI_FLASH_SendByte(unsigned char byte)
    317          {
    318          	
    319          	// Loop while DR register in not emplty 
    320          	while(SPI_I2S_GetFlagStatus(SPI1, SPI_I2S_FLAG_TXE) == RESET);
    321          	// Send byte through the SPI1 peripheral 
    322          	SPI_I2S_SendData(SPI1, byte);
    323          	// Wait to receive a byte 
    324          	while(SPI_I2S_GetFlagStatus(SPI1, SPI_I2S_FLAG_RXNE) == RESET);
    325          	// Return the byte read from the SPI bus 
    326          	
    327          	return SPI_I2S_ReceiveData(SPI1);
    328          }
