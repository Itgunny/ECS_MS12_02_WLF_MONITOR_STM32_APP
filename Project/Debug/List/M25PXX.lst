    342          void SPI_FLASH_WaitForWriteEnd(void)
    343          {
    344          	unsigned char FLASH_Status = 0;
    345          
    346              //  ++, kutelf, 140801    
    347              //unsigned int TimeOut = 0;
    348              //  --, kutelf, 140801
    349              
    350          	// Select the FLASH: Chip Select low 
    351          	SPI_FLASH_CS_LOW();
    352          
    353          	// Send "Read Status Register" instruction 
    354          	SPI_Transfer(RDSR);
    355              
    356          	// Loop as long as the memory is busy with a write cycle 
    357            
    358          	do
    359          	{
    360          		// Send a dummy byte to generate the clock needed by the FLASH and put the value of the status register in FLASH_Status variable 
    361          		FLASH_Status = SPI_Transfer(Dummy_Byte);
    362          
    363                  //  ++, kutelf, 140801
    364                  //  SPI Flash가 문제가 있을 경우 - 무한루프에서 빠져나오지 못할 수 있음. 
    365                  //if (TimeOut++ > 2000)
    366                  //  break;
    367                  //  --, kutelf, 140801
    368          
    369          	} while((FLASH_Status & WIP_Flag) == SET); // Write in progress 
    370          
    371          	// Deselect the FLASH: Chip Select high 
    372          
    373          	SPI_FLASH_CS_HIGH();
    374          }
