    294          
    295          unsigned char  SPI_Transfer(u8 data)
    296          {      
    297          	unsigned char i;
    298          	unsigned char result;
    299          
    300          	result = 0;
    301          
    302          	for (i = 0; i < 8; i++)
    303          	{
    304          		SPI_FLASH_SCK_LOW();
    305          		(data & (1 << (7- i))) ? SPI_FLASH_MOSI_HIGH():SPI_FLASH_MOSI_LOW();
    306          		SPI_FLASH_SCK_HIGH();
    307          
    308          		result <<= 1;
    309          		result |= GPIO_ReadInputDataBit(SFLASH_SPI1_PORT,SFLASH_SPI1_MISO);
    310          	}
    311          
    312          	return result;
    313              
    314          }
