    260          void CAN_TX_Data(u8* Data)
    261          {
    262          	//u8 TransmitMailbox;
    263          	CanTxMsg TxMessage;
    264          	
    265          	TxMessage.ExtId=((unsigned long)(Identifier.Priority) << 26)+((unsigned long)(Identifier.Data_Page) << 24)+((unsigned long)(Identifier.PDU_Format) << 16)
    266          				                      + ((unsigned long)(Identifier.PDU_Specific) << 8) + ((unsigned long)(Identifier.Source_Address) << 0) ;
    267          			
    268          	TxMessage.IDE = CAN_ID_EXT;
    269          	TxMessage.RTR = CAN_RTR_DATA;
    270          	TxMessage.DLC = 8;
    271          
    272          	memcpy((u8*)&TxMessage.Data[0], (u8*)&Data[0], 8);
    273          
    274          	
    275          	CAN_Transmit(CAN1,&TxMessage);
    276          }
