    281          void SendMultiPacket_MachInfo(void)
    282          {
    283          	u8 tmpBuf1[8];
    284          	
    285          	SetCanID(235, 255, 7);
    286          
    287          	tmpBuf1[0] = MachInfoTotalPacketNum;
    288          	
    289          	memcpy(&tmpBuf1[1], &tmpMcuInfoData[(MachInfoTotalPacketNum-1)*7], 7);
    290          	CAN_TX_Data(&tmpBuf1[0]);
    291          	MachInfoTotalPacketNum++;
    292          
    293          	if(MachInfoTotalPacketNum > 3)	// HCESPN 1490+1491 = 17Bytes
    294          	{
    295          		MachInfoSendCnt++;
    296          		Flag_1Sec_MachInfo = 0;
    297          		MachInfoTotalPacketNum = 0;
    298          		if((Flag_SerialRxMsg & RX_MSG69_M) != 0)	// 69 - Multi Packet
    299          			MultiPacketSendOrder = 0;
    300          		else if(MoniInfoSendCnt <= 60)
    301          			MultiPacketSendOrder = 2;
    302          	}		
    303          }
