    365          void CAN_TX_Data(u8* Data)
    366          {
    367          	//u8 TransmitMailbox;
    368          	CanTxMsg TxMessage;
    369          	
    370          	TxMessage.ExtId=((unsigned long)(Identifier.Priority) << 26)+((unsigned long)(Identifier.Data_Page) << 24)+((unsigned long)(Identifier.PDU_Format) << 16)
    371          				                      + ((unsigned long)(Identifier.PDU_Specific) << 8) + ((unsigned long)(Identifier.Source_Address) << 0) ;
    372          			
    373          	TxMessage.IDE = CAN_ID_EXT;
    374          	TxMessage.RTR = CAN_RTR_DATA;
    375          	TxMessage.DLC = 8;
    376          
    377          	memcpy((u8*)&TxMessage.Data[0], (u8*)&Data[0], 8);
    378          
    379          	
    380          	CAN_Transmit(CAN1,&TxMessage);
    381          }
