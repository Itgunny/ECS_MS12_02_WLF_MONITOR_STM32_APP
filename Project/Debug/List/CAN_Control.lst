    367          void SendTP_CM_BAM_MultiPacket_69(void)
    368          {
    369          	static u8 AsteriskCnt = 0;
    370          	u8 i;
    371          	
    372          	// TP.CM_BAM
    373          	SetCanID(236, 255, 7);
    374          
    375          	tp_cm_bam->Control = 32;	// Control Byte
    376          	
    377          	tp_cm_bam->TotMsgSize = 7;	// Password Info - 4 Bytes + Asterisk (3 EA)
    378          
    379          	for(i = 0 ; i < 13 ; i++)
    380          	{
    381          		if(Uart2_RxMsg_Multi_69[i+4] != 0x2A)
    382          			tp_cm_bam->TotMsgSize++;
    383          		else
    384          			AsteriskCnt++;
    385          
    386          		if(AsteriskCnt >= 3)
    387          		{
    388          			AsteriskCnt = 0;
    389          			break;
    390          		}
    391          	}
    392          
    393          	tp_cm_bam->TotPacketNum = (tp_cm_bam->TotMsgSize > 14) ? 3 : 2;
    394          	tp_cm_bam_TotPacketNum = T_Packet_No = tp_cm_bam->TotPacketNum;
    395          
    396          	tp_cm_bam->Reserved = 0xff;
    397          
    398          	//tp_cm_bam->pgn_low = 0xEC00;
    399          	tp_cm_bam->pgn_low = 0xFF45;
    400          	tp_cm_bam->pgn_high = 0;
    401          
    402          	CAN_TX_Data(&tmpBuf[0]);
    403          }
