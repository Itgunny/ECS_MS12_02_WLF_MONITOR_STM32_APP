    299          void SendMultiPacket_MachInfo(void)
    300          {
    301          	u8 tmpBuf1[8];
    302          	
    303          	SetCanID(235, 255, 7);
    304          
    305          	tmpBuf1[0] = MachInfoTotalPacketNum;
    306          	
    307          	memcpy(&tmpBuf1[1], &tmpMcuInfoData[(MachInfoTotalPacketNum-1)*7], 7);
    308          	CAN_TX_Data(&tmpBuf1[0]);
    309          	MachInfoTotalPacketNum++;
    310          
    311          	if(MachInfoTotalPacketNum > 3)	// HCESPN 1490+1491 = 17Bytes
    312          	{
    313          		MachInfoSendCnt++;
    314          		Flag_1Sec_MachInfo = 0;
    315          		MachInfoTotalPacketNum = 0;
    316          		if((Flag_SerialRxMsg & RX_MSG69_M) != 0)	// 69 - Multi Packet
    317          			MultiPacketSendOrder = 0;
    318          		else if(MoniInfoSendCnt <= 60)
    319          			MultiPacketSendOrder = 2;
    320          	}		
    321          }
