    456          void CheckASDataLen(void)
    457          {
    458          	u8 i;
    459          
    460          	as_data_len = 0;
    461          	memset((u8*)&tmpbuf_AS[0], 0x2A, 13);
    462          
    463          	for(i = 0 ; i < 12 ; i+=2)
    464          	{
    465          		tmpbuf_AS[i] = Uart2_RxMsg_AS_Phone_Data[i/2] & 0x0F;
    466          		tmpbuf_AS[i+1] = (Uart2_RxMsg_AS_Phone_Data[i/2] & 0xF0) >> 4;
    467          	}
    468          
    469          
    470          	for(i = 0 ; i < 12 ; i++)
    471          	{
    472          		if(tmpbuf_AS[i] != 0x0F)
    473          			as_data_len++;
    474          		else
    475          		{
    476          			tmpbuf_AS[i] = 0x2A;
    477          			break;
    478          		}
    479          	}
    480          
    481          	check_as_data_len = 1;
    482          }
