    474          void CheckASDataLen(void)
    475          {
    476          	u8 i;
    477          
    478          	as_data_len = 0;
    479          	memset((u8*)&tmpbuf_AS[0], 0x2A, 13);
    480          
    481          	for(i = 0 ; i < 12 ; i+=2)
    482          	{
    483          		tmpbuf_AS[i] = Uart2_RxMsg_AS_Phone_Data[i/2] & 0x0F;
    484          		tmpbuf_AS[i+1] = (Uart2_RxMsg_AS_Phone_Data[i/2] & 0xF0) >> 4;
    485          	}
    486          
    487          
