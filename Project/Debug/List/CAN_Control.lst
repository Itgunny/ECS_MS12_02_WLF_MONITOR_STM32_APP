    479          void CheckASDataLen(void)
    480          {
    481          	u8 i;
    482          
    483          	as_data_len = 0;
    484          	memset((u8*)&tmpbuf_AS[0], 0x2A, 13);
    485          
    486          	for(i = 0 ; i < 12 ; i+=2)
    487          	{
    488          		tmpbuf_AS[i] = Uart2_RxMsg_AS_Phone_Data[i/2] & 0x0F;
    489          		tmpbuf_AS[i+1] = (Uart2_RxMsg_AS_Phone_Data[i/2] & 0xF0) >> 4;
    490          	}
    491          
    492          
    493          	for(i = 0 ; i < 12 ; i++)
    494          	{
    495          		if(tmpbuf_AS[i] != 0x0F)
