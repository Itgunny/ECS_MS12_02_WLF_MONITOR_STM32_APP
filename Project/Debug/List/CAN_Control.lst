    225          void SetCanID(u8 PF, u8 PS, u8 Priority)
    226          {
    227          	if(PF != 0)
    228          		Identifier.PDU_Format = PF;	
    229          
    230          	if(PS != 0)
    231          		Identifier.PDU_Specific = PS;	
    232          
