    349          void SendTP_CM_BAM_MultiPacket_69(void)
    350          {
    351          	static u8 AsteriskCnt = 0;
    352          	u8 i;
    353          	
    354          	// TP.CM_BAM
    355          	SetCanID(236, 255, 7);
    356          
    357          	tp_cm_bam->Control = 32;	// Control Byte
    358          	
    359          	tp_cm_bam->TotMsgSize = 7;	// Password Info - 4 Bytes + Asterisk (3 EA)
    360          
    361          	for(i = 0 ; i < 13 ; i++)
    362          	{
    363          		if(Uart2_RxMsg_Multi_69[i+4] != 0x2A)
    364          			tp_cm_bam->TotMsgSize++;
    365          		else
    366          			AsteriskCnt++;
    367          
    368          		if(AsteriskCnt >= 3)
    369          		{
    370          			AsteriskCnt = 0;
    371          			break;
    372          		}
    373          	}
    374          
    375          	tp_cm_bam->TotPacketNum = (tp_cm_bam->TotMsgSize > 14) ? 3 : 2;
    376          	tp_cm_bam_TotPacketNum = T_Packet_No = tp_cm_bam->TotPacketNum;
    377          
    378          	tp_cm_bam->Reserved = 0xff;
    379          
    380          	//tp_cm_bam->pgn_low = 0xEC00;
    381          	tp_cm_bam->pgn_low = 0xFF45;
    382          	tp_cm_bam->pgn_high = 0;
    383          
    384          	CAN_TX_Data(&tmpBuf[0]);
    385          }
