    243          void SetCanID(u8 PF, u8 PS, u8 Priority)
    244          {
    245          	if(PF != 0)
    246          		Identifier.PDU_Format = PF;	
    247          
    248          	if(PS != 0)
    249          		Identifier.PDU_Specific = PS;	
    250          
    251          	if(Priority != 0)
    252          		Identifier.Priority = Priority;	
    253          }
