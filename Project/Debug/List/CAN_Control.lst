    237          void CAN_TX_Data(u8* Data)
    238          {
    239          	//u8 TransmitMailbox;
    240          	CanTxMsg TxMessage;
    241          	
    242          	TxMessage.ExtId=((unsigned long)(Identifier.Priority) << 26)+((unsigned long)(Identifier.Data_Page) << 24)+((unsigned long)(Identifier.PDU_Format) << 16)
    243          				                      + ((unsigned long)(Identifier.PDU_Specific) << 8) + ((unsigned long)(Identifier.Source_Address) << 0) ;
    244          			
    245          	TxMessage.IDE = CAN_ID_EXT;
    246          	TxMessage.RTR = CAN_RTR_DATA;
    247          	TxMessage.DLC = 8;
    248          
    249          	memcpy((u8*)&TxMessage.Data[0], (u8*)&Data[0], 8);
    250          
    251          	
    252          	CAN_Transmit(CAN1,&TxMessage);
    253          }
