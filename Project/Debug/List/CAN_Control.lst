    385          
    386          void CAN_TX(void)
    387          {
    388          	CanTxMsg TxMessage;
    389          	unsigned char i;
    390          	static unsigned short Tx_Rountine_Count_CAN;
    391          
    392          	
    393          	if((++Tx_Rountine_Count_CAN%2)==0)
    394          	{
    395          		if (CAN_Message_Ring_Buffer_Tx_Single.Head != CAN_Message_Ring_Buffer_Tx_Single.Tail)
    396          		{
    397          			
    398          			TxMessage.ExtId = (CAN_Message_Ring_Buffer_Tx_Single.Message[CAN_Message_Ring_Buffer_Tx_Single.Tail].Priority<<24)+
    399          							(CAN_Message_Ring_Buffer_Tx_Single.Message[CAN_Message_Ring_Buffer_Tx_Single.Tail].PDU_Format<<16)+
    400          							(CAN_Message_Ring_Buffer_Tx_Single.Message[CAN_Message_Ring_Buffer_Tx_Single.Tail].PDU_Specific<<8)+
    401          							CAN_Message_Ring_Buffer_Tx_Single.Message[CAN_Message_Ring_Buffer_Tx_Single.Tail].Source_Address;
    402          			TxMessage.IDE = CAN_ID_EXT;
    403          			TxMessage.RTR = CAN_RTR_DATA;
    404          			TxMessage.DLC = 8;
    405          
    406          			
    407          			for (i = 0; i < 8; i++)
    408          			{
    409          				TxMessage.Data[i] = CAN_Message_Ring_Buffer_Tx_Single.Message[CAN_Message_Ring_Buffer_Tx_Single.Tail].Data[i];
    410          			}
    411          
    412          			can_status = CAN_Transmit(CAN1,&TxMessage);
    413          
    414          							
    415          			if(can_status !=CAN_TxStatus_NoMailBox)
    416          			{
    417          				if (++(CAN_Message_Ring_Buffer_Tx_Single.Tail) >= MAX_CAN_TX_DATA_SINGLE)
    418          					CAN_Message_Ring_Buffer_Tx_Single.Tail = 0;
    419          			}
    420          			else
    421          			{
    422          				can_status_buf[can_tx_error_count++] = can_status;
    423          				if(can_tx_error_count>100)	can_tx_error_count=0;
    424          			}
    425          
    426          
    427          		}
    428          		
    429          	}
    430          
    431          
    432          	 //if(Tx_Rountine_Count_CAN++ > 500)
    433          	 //{
    434          	//		Tx_Rountine_Count_CAN = 0;
    435          	 //}
    436                 	
    437          }
    438          void MonitorStatus_CAN_TX(void)
    439          {
    440          	struct st_CAN_Message1 Send_Message;
    441          
    442          
    443          	Send_Message.Priority = 0x18;
    444          	Send_Message.PDU_Format = 0xFF;		// 255
    445          	Send_Message.PDU_Specific = 0x2F;	// 47
