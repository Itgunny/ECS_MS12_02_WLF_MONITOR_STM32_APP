    372          void SendTP_CM_BAM_MultiPacket_69(void)
    373          {
    374          	static u8 AsteriskCnt = 0;
    375          	u8 i;
    376          	
    377          	// TP.CM_BAM
    378          	SetCanID(236, 255, 7);
    379          
    380          	tp_cm_bam->Control = 32;	// Control Byte
    381          	
    382          	tp_cm_bam->TotMsgSize = 7;	// Password Info - 4 Bytes + Asterisk (3 EA)
    383          
    384          	for(i = 0 ; i < 13 ; i++)
    385          	{
    386          		if(Uart2_RxMsg_Multi_69[i+4] != 0x2A)
    387          			tp_cm_bam->TotMsgSize++;
    388          		else
    389          			AsteriskCnt++;
    390          
    391          		if(AsteriskCnt >= 3)
    392          		{
    393          			AsteriskCnt = 0;
    394          			break;
    395          		}
    396          	}
    397          
    398          	tp_cm_bam->TotPacketNum = (tp_cm_bam->TotMsgSize > 14) ? 3 : 2;
    399          	tp_cm_bam_TotPacketNum = T_Packet_No = tp_cm_bam->TotPacketNum;
    400          
    401          	tp_cm_bam->Reserved = 0xff;
    402          
    403          	//tp_cm_bam->pgn_low = 0xEC00;
    404          	tp_cm_bam->pgn_low = 0xFF45;
    405          	tp_cm_bam->pgn_high = 0;
    406          
    407          	CAN_TX_Data(&tmpBuf[0]);
    408          }
