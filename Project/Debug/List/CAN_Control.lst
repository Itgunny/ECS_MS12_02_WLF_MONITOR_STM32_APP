    304          void SendMultiPacket_MachInfo(void)
    305          {
    306          	u8 tmpBuf1[8];
    307          	
    308          	SetCanID(235, 255, 7);
    309          
    310          	tmpBuf1[0] = MachInfoTotalPacketNum;
    311          	
    312          	memcpy(&tmpBuf1[1], &tmpMcuInfoData[(MachInfoTotalPacketNum-1)*7], 7);
    313          	CAN_TX_Data(&tmpBuf1[0]);
    314          	MachInfoTotalPacketNum++;
    315          
    316          	if(MachInfoTotalPacketNum > 3)	// HCESPN 1490+1491 = 17Bytes
    317          	{
    318          		MachInfoSendCnt++;
    319          		Flag_1Sec_MachInfo = 0;
    320          		MachInfoTotalPacketNum = 0;
    321          		if((Flag_SerialRxMsg & RX_MSG69_M) != 0)	// 69 - Multi Packet
    322          			MultiPacketSendOrder = 0;
    323          		else if(MoniInfoSendCnt <= 60)
    324          			MultiPacketSendOrder = 2;
    325          	}		
    326          }
