    255          void CAN_TX_Data(u8* Data)
    256          {
    257          	//u8 TransmitMailbox;
    258          	CanTxMsg TxMessage;
    259          	
    260          	TxMessage.ExtId=((unsigned long)(Identifier.Priority) << 26)+((unsigned long)(Identifier.Data_Page) << 24)+((unsigned long)(Identifier.PDU_Format) << 16)
    261          				                      + ((unsigned long)(Identifier.PDU_Specific) << 8) + ((unsigned long)(Identifier.Source_Address) << 0) ;
    262          			
    263          	TxMessage.IDE = CAN_ID_EXT;
    264          	TxMessage.RTR = CAN_RTR_DATA;
    265          	TxMessage.DLC = 8;
    266          
    267          	memcpy((u8*)&TxMessage.Data[0], (u8*)&Data[0], 8);
    268          
    269          	
    270          	CAN_Transmit(CAN1,&TxMessage);
    271          }
