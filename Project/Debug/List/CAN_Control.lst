    344          void CAN_TX_Data(u8* Data)
    345          {
    346          	//u8 TransmitMailbox;
    347          	CanTxMsg TxMessage;
    348          	
    349          	TxMessage.ExtId=((unsigned long)(Identifier.Priority) << 26)+((unsigned long)(Identifier.Data_Page) << 24)+((unsigned long)(Identifier.PDU_Format) << 16)
    350          				                      + ((unsigned long)(Identifier.PDU_Specific) << 8) + ((unsigned long)(Identifier.Source_Address) << 0) ;
    351          			
    352          	TxMessage.IDE = CAN_ID_EXT;
    353          	TxMessage.RTR = CAN_RTR_DATA;
    354          	TxMessage.DLC = 8;
    355          
    356          	memcpy((u8*)&TxMessage.Data[0], (u8*)&Data[0], 8);
    357          
    358          	
    359          	CAN_Transmit(CAN1,&TxMessage);
    360          }
