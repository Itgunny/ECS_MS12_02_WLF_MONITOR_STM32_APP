    893          
    894          					break;
    895          				case VersionCMD:
    896          					
    897          					Temp[0] = 0x02;				
    898          					Temp[1] = VersionRES;				
    899          					Temp[2] = ((VERSION_HIGH  << 4) & 0xF0 ) + (VERSION_LOW & 0x0F);	
    900          					Temp[3] = ((VERSION_SUB_HIGH  << 4) & 0xF0 )+ (VERSION_SUB_LOW & 0x0F);	
