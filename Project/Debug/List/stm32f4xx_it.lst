    690          			        	Uart2_SerialTxCnt = 0;
    691          
    692          					//  Disable the USART3 Transmit interrupt
    693          					USART_ITConfig(USART2, USART_IT_TXE, DISABLE);
    694          				}
    695          			}
    696          			else
    697          			{
    698          				Uart2_SerialTxCnt = 0;
    699          				pReadBufPos += 12;
    700          			}
    701          		}  
    702          	}
