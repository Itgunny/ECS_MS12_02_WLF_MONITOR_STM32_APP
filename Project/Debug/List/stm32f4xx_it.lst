    920          					WL9FM_RTC.Second= WL9FM_USART_DATA.COM4_RxBuf[8];
    921          					WRITE_RTC(WL9FM_RTC);
    922          					break;
    923          
    924          				case SMKCMD:
    925          					memcpy(&Uart2_RxMsg_Smk_Reg_Eli[0], &WL9FM_USART_DATA.COM4_RxBuf[2], 8);
    926          					break;
    927          				case CANUPDATECMD:
    928          					CANUpdateFlag = WL9FM_USART_DATA.COM4_RxBuf[2];
    929          					CANUpdateSA = WL9FM_USART_DATA.COM4_RxBuf[3];
    930          					break;
    931          					
    932          					
    933          				default :
    934          					break;								
    935          			}
    936                          }						
    937          
    938          		//memset(WL9FM_USART_DATA.COM4_RxBuf, 0x0, Serial_COM4_RxSize);					
    939          				break;
    940          		
    941          		default :
    942          
    943          			WL9FM_USART_INDEX.COM4_RxCnt++;
    944          			break;
    945          	}	
