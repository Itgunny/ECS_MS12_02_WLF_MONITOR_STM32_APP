    612              
    613              //  BUZZER Status가 "2" 일 경우 BUZZER OnTime 만큼 BUZZER On
    614              if (WL9FM_BUZZER.Status == 2)
    615              {
    616                  if (WL9FM_BUZZER.OnCnt++ < WL9FM_BUZZER.OnTime)
    617                  {
    618                      Buzzer_On();
    619                  }
    620                  else
    621                  {
    622                      WL9FM_BUZZER.Status = 0;               
    623                  }
    624              }        
    625              //  BUZZER Status가 "1" 일 경우 UnLimit BUZZER On
    626              else if (WL9FM_BUZZER.Status == 1)
    627              {
    628                  Buzzer_On();    
    629              }    
    630              else
    631              {
    632                  Buzzer_Off();
    633                  
    634                  WL9FM_BUZZER.Status = 0;               
    635                  WL9FM_BUZZER.OnTime = 0;
    636                  WL9FM_BUZZER.OnCnt  = 0;
    637              }
    638          
    639          #if 1
    640          	CommErrCnt++;
    641          
    642          	if(CommErrCnt >= 1000)
    643          		CommErrCnt = 1001;
    644          #endif
    645          }
