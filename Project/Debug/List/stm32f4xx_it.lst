    450          /**
    451            * @brief  This function handles CAN1 global interrupt request.
    452            * @param  None
    453            * @retval None
    454            */
    455          void CAN1_RX0_IRQHandler(void)
    456          {
    457          	u32 PF; 
    458          	u32 PS;
    459          	u32 PGN;
    460          
    461          	CAN_Receive(CAN1,CAN_FIFO0,&RxMsg);
    462          				
    463          	Iden.Source_Address = (RxMsg.ExtId & 0x000000ff)  >> 0;
    464          	Iden.PDU_Specific = (RxMsg.ExtId  & 0x0000ff00) >> 8;
    465          	
    466          	// Iden.Source_Address == 71	 -->>	MCU
    467          	// Iden.Source_Address == 228	 -->>	EHCU	
    468          	// Iden.Source_Address == 0x29	 -->>	Smart Key
    469          	// Iden.Source_Address == 23	 -->>	Cluster
    470          	// Iden.Source_Address == 221	 -->>	RCU
    471          	// Iden.Source_Address == 0	 -->>	ECM
    472          	// Iden.Source_Address == 3	 -->>	TCU
    473          	
    474          	if((Iden.Source_Address == 0x47) || (Iden.Source_Address == 0x17) || (Iden.Source_Address == 0x29) || 
    475          		(Iden.Source_Address == 0xE4) || (Iden.Source_Address == 0xDD)|| (Iden.Source_Address == 0x4a)|| (Iden.Source_Address == 0xf4)
    476          		|| (Iden.Source_Address == 0x00)|| (Iden.Source_Address == 0x03) || (Iden.Source_Address == 0x02))
    477          		{
    478          		#if 1
    479          			
    480          			if(CANUpdateFlag == 0)
    481          			{
    482          				PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    483          				if((PF == 254) || (PF == 255) || (PF == 239) )
    484          				{	
    485          					
    486          					if(Iden.PDU_Specific == 232)	// Smart Key
    487          					{
    488          						smk_flag_data.recv_resp_packet |= RESPONSE_AUTHENTICATION;	
    489          						//smk_flag_data.recv_resp_packet |= 0x0100; 
    490          
    491          						memcpy((u8*)&recv_smartkey, (u8*)&RxMsg.Data[0], 8);
    492          					}
    493          					else
    494          					{
    495          		
    496          						//if(Iden.PDU_Specific == 0x9b)
    497          							CAN_RX(RxMsg);
    498          					}
    499          				}
    500          				
    501          				else
    502          				{                    
    503          	                		CAN_RX(RxMsg);
    504          				}
    505          		
    506          				CommErrCnt = 0;
    507          			}
    508          			else if(CANUpdateFlag == 1)
    509          			{
    510          				if(Iden.Source_Address == CANUpdateSA)
    511          				{
    512          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    513          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    514          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    515          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    516          					{
    517          						CAN_RX(RxMsg);
    518          					}
    519          
    520          				}
    521          				
    522          			}
    523          		#else
    524          			if(CANUpdateFlag == 1)
    525          			{
    526          				if(Iden.Source_Address == CANUpdateSA)
    527          				{
    528          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    529          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    530          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    531          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    532          					{
    533          						//OperateRingBuffer();
    534          						CAN_RX(RxMsg);
    535          						CANRXIndex++;
    536          					}
    537          
    538          				}
    539          				
    540          			}
    541          		#endif
    542          			
    543          			
    544          
    545          			
    546          			
    547          		}
    548          
    549          }
