    246          void OperateRingBuffer(void)
    247          {
    248          	if(pWriteBufPos >= (RING_BUF_SIZE-1))	// End of Ring Buffer
    249          		pWriteBufPos = 0;
    250          
    251          	memcpy(&ring_buf[pWriteBufPos], (u8*)&RxMsg.ExtId, 4);
    252          
    253          	pWriteBufPos += 4;
    254          
    255          	memcpy(&ring_buf[pWriteBufPos], (u8*)&RxMsg.Data, 8);
    256          	
    257          	pWriteBufPos += 8;	
    258          
    259          	USART_ITConfig(USART2, USART_IT_TXE, ENABLE);
    260          	
    261          }
    262          void RTCSend(void)
    263          {
    264          	if(pWriteBufPos >= (RING_BUF_SIZE-1)) // End of Ring Buffer
    265          		pWriteBufPos = 0;
    266          
    267          	memcpy(&ring_buf[pWriteBufPos], (u8*)&SerialMsgRTC[0], 12);
    268          
    269          	pWriteBufPos += 12;
    270          
    271          	USART_ITConfig(USART2, USART_IT_TXE, ENABLE);
    272          }
