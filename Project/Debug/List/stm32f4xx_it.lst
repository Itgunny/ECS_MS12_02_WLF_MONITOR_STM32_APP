    245          void OperateRingBuffer(void)
    246          {
    247          	if(pWriteBufPos >= (RING_BUF_SIZE-1))	// End of Ring Buffer
    248          		pWriteBufPos = 0;
    249          
    250          	memcpy(&ring_buf[pWriteBufPos], (u8*)&RxMsg.ExtId, 4);
    251          
    252          	pWriteBufPos += 4;
    253          
    254          	memcpy(&ring_buf[pWriteBufPos], (u8*)&RxMsg.Data, 8);
    255          	
    256          	pWriteBufPos += 8;	
    257          
    258          	USART_ITConfig(USART2, USART_IT_TXE, ENABLE);
    259          	
    260          }
    261          void RTCSend(void)
    262          {
    263          	if(pWriteBufPos >= (RING_BUF_SIZE-1)) // End of Ring Buffer
    264          		pWriteBufPos = 0;
    265          
    266          	memcpy(&ring_buf[pWriteBufPos], (u8*)&SerialMsgRTC[0], 12);
    267          
    268          	pWriteBufPos += 12;
    269          
    270          	USART_ITConfig(USART2, USART_IT_TXE, ENABLE);
    271          }
