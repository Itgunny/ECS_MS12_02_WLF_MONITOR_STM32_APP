    454          /**
    455            * @brief  This function handles CAN1 global interrupt request.
    456            * @param  None
    457            * @retval None
    458            */
    459          void CAN1_RX0_IRQHandler(void)
    460          {
    461          	u32 PF; 
    462          	u32 PS;
    463          	u32 PGN;
    464          
    465          	CAN_Receive(CAN1,CAN_FIFO0,&RxMsg);
    466          				
    467          	Iden.Source_Address = (RxMsg.ExtId & 0x000000ff)  >> 0;
    468          	Iden.PDU_Specific = (RxMsg.ExtId  & 0x0000ff00) >> 8;
    469          	
    470          	// Iden.Source_Address == 71	 -->>	MCU
    471          	// Iden.Source_Address == 228	 -->>	EHCU	
    472          	// Iden.Source_Address == 0x29	 -->>	Smart Key
    473          	// Iden.Source_Address == 23	 -->>	Cluster
    474          	// Iden.Source_Address == 221	 -->>	RCU
    475          	// Iden.Source_Address == 0	 -->>	ECM
    476          	// Iden.Source_Address == 3	 -->>	TCU
    477          	
    478          	if((Iden.Source_Address == 0x47) || (Iden.Source_Address == 0x17) || (Iden.Source_Address == 0x29) || 
    479          		(Iden.Source_Address == 0xE4) || (Iden.Source_Address == 0xDD)|| (Iden.Source_Address == 0x4a)|| (Iden.Source_Address == 0xf4)
    480          		|| (Iden.Source_Address == 0x00)|| (Iden.Source_Address == 0x03) || (Iden.Source_Address == 0x02))
    481          		{
    482          		#if 1
    483          			
    484          			if(CANUpdateFlag == 0)
    485          			{
    486          				PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    487          				if((PF == 254) || (PF == 255) || (PF == 239) )
    488          				{	
    489          					
    490          					if(Iden.PDU_Specific == 232)	// Smart Key
    491          					{
    492          						smk_flag_data.recv_resp_packet |= RESPONSE_AUTHENTICATION;	
    493          						//smk_flag_data.recv_resp_packet |= 0x0100; 
    494          
    495          						memcpy((u8*)&recv_smartkey, (u8*)&RxMsg.Data[0], 8);
    496          					}
    497          					else
    498          					{
    499          		
    500          						//if(Iden.PDU_Specific == 0x9b)
    501          							CAN_RX(RxMsg);
    502          					}
    503          				}
    504          				
    505          				else
    506          				{                    
    507          	                		CAN_RX(RxMsg);
    508          				}
    509          		
    510          				CommErrCnt = 0;
    511          			}
    512          			else if(CANUpdateFlag == 1)
    513          			{
    514          				if(Iden.Source_Address == CANUpdateSA)
    515          				{
    516          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    517          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    518          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    519          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    520          					{
    521          						CAN_RX(RxMsg);
    522          					}
    523          
    524          				}
    525          				
    526          			}
    527          		#else
    528          			if(CANUpdateFlag == 1)
    529          			{
    530          				if(Iden.Source_Address == CANUpdateSA)
    531          				{
    532          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    533          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    534          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    535          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    536          					{
    537          						//OperateRingBuffer();
    538          						CAN_RX(RxMsg);
    539          						CANRXIndex++;
    540          					}
    541          
    542          				}
    543          				
    544          			}
    545          		#endif
    546          			
    547          			
    548          
    549          			
    550          			
    551          		}
    552          
    553          }
