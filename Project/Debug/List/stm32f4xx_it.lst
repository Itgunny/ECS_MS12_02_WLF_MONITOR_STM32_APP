    501          	// Iden.Source_Address == 0x29	 -->>	Smart Key
    502          	// Iden.Source_Address == 23	 -->>	Cluster
    503          	// Iden.Source_Address == 221	 -->>	RCU
    504          	// Iden.Source_Address == 0	 -->>	ECM
    505          	// Iden.Source_Address == 3	 -->>	TCU
    506          	// Iden.Source_Address == 25	 -->>	ACU
    507          	// Iden.Source_Address == 52	 -->>	BKCU
    508          	if((Iden.Source_Address == 0x47) || (Iden.Source_Address == 0x17) || (Iden.Source_Address == 0x29) || 
    509          		(Iden.Source_Address == 0xE4) || (Iden.Source_Address == 0xDD)|| (Iden.Source_Address == 0x4a)|| (Iden.Source_Address == 0xf4)
    510          		|| (Iden.Source_Address == 0x00)|| (Iden.Source_Address == 0x03) || (Iden.Source_Address == 0x02) || (Iden.Source_Address == 0x19)
    511          		|| (Iden.Source_Address == 0x34))
    512          		{
    513          		#if 1
    514          			
    515          			if(CANUpdateFlag == 0)
    516          			{
    517          				PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    518          				if((PF == 254) || (PF == 255) || (PF == 239) )
    519          				{	
    520          					
    521          					if(Iden.PDU_Specific == 232)	// Smart Key
    522          					{
    523          						smk_flag_data.recv_resp_packet |= RESPONSE_AUTHENTICATION;	
    524          						//smk_flag_data.recv_resp_packet |= 0x0100; 
    525          						memcpy((u8*)&recv_smartkey, (u8*)&RxMsg.Data[0], 8);
    526          					}
    527          					else
    528          					{
    529          		
    530          						//if(Iden.PDU_Specific == 0x9b)
    531          							CAN_RX(RxMsg);
    532          					}
    533          				}
    534          				
    535          				else
    536          				{                    
    537          	                		CAN_RX(RxMsg);
    538          				}
    539          		
    540          				CommErrCnt = 0;
    541          			}
    542          			else if(CANUpdateFlag == 1)
    543          			{
    544          				if(Iden.Source_Address == CANUpdateSA)
    545          				{
    546          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    547          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    548          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    549          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    550          					{
    551          						CAN_RX(RxMsg);
    552          					}
    553          
    554          				}
    555          				if(Iden.Source_Address == 0x47)
    556          				{
    557          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    558          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    559          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    560          					
    561          					if(PGN == 0xEF28 && RxMsg.Data[0] == 122)
    562          					{
    563          						CAN_RX(RxMsg);
    564          					}
    565          				}
    566          				if(Iden.Source_Address == 0x34){
    567          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    568          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    569          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    570          					if(PGN == 0xFFFA || PGN == 0xFE34){
    571          						CAN_RX(RxMsg);
    572          					}
    573          				}
    574          			}
    575          		#else
    576          			if(CANUpdateFlag == 1)
    577          			{
    578          				if(Iden.Source_Address == CANUpdateSA)
    579          				{
    580          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    581          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    582          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    583          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    584          					{
    585          						//OperateRingBuffer();
    586          						CAN_RX(RxMsg);
    587          						CANRXIndex++;
    588          					}
    589          
    590          				}
    591          				
    592          			}
    593          		#endif
    594          			
    595          			
    596          
    597          			
    598          			
    599          		}
    600          
    601          }
