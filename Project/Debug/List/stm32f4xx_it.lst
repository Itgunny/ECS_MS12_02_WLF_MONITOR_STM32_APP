    332          
    333          			if (WL9FM_TIME.Cnt_1mSec % 100 == 0)         //  100msec
    334          			{
    335          				WL9FM_TIME.Flag_100mSec = 1;
    336          
    337          				if (WL9FM_TIME.Cnt_1mSec % 1000 == 0)    //  1000 msec
    338          				{
    339          					WL9FM_TIME.Flag_1Sec = 1;
    340          					WL9FM_TIME.Cnt_1mSec = 0;
    341          				}
    342          			}
    343          		}
    344          	}    
    345          }
