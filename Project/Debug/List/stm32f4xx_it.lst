    551          /**
    552            * @brief  This function handles TIM4 global interrupt request.
    553            * @param  None
    554            * @retval None
    555            */
    556          void TIM4_IRQHandler(void)  //  10msec Timer / TimeBase UP Counter
    557          {
    558              TIM_ClearITPendingBit(TIM4, TIM_IT_Update);
    559              
    560              //  BUZZER Status가 "2" 일 경우 BUZZER OnTime 만큼 BUZZER On
    561              if (WL9FM_BUZZER.Status == 2)
    562              {
    563                  if (WL9FM_BUZZER.OnCnt++ < WL9FM_BUZZER.OnTime)
    564                  {
    565                      Buzzer_On();
    566                  }
