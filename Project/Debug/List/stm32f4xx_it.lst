    392            * @brief  This function handles SysTick Handler.
    393            * @param  None
    394            * @retval None
    395            */
    396          void SysTick_Handler(void)
    397          {
    398            	TimeDelay_Decrement();
    399          
    400              ++WL9FM_TIME.Cnt_1mSec;
    401          
    402          	if (WL9FM_TIME.Cnt_1mSec % 1 == 0)                   //  1msec
    403          	{
    404                  WL9FM_TIME.Flag_1mSec = 1;
    405          
    406          		if (WL9FM_TIME.Cnt_1mSec % 10 == 0)              //  10msec
    407          		{
    408          			WL9FM_TIME.Flag_10mSec = 1;
    409          
    410          			if (WL9FM_TIME.Cnt_1mSec % 100 == 0)         //  100msec
    411          			{
    412          				WL9FM_TIME.Flag_100mSec = 1;
    413          
    414          				if(WL9FM_TIME.Cnt_1mSec % 500 == 0)			// 500msec
    415          				{
    416          					WL9FM_TIME.Flag_500mSec = 1;
    417          					if (WL9FM_TIME.Cnt_1mSec % 1000 == 0)    //  1000 msec
    418          					{
    419          						WL9FM_TIME.Flag_1Sec = 1;
    420          						WL9FM_TIME.Cnt_1mSec = 0;
    421          					}
    422          				}
    423          
    424          				
    425          			}
    426          		}
    427          	}    
    428          }
