    241          void OperateRingBuffer(void)
    242          {
    243          	if(pWriteBufPos >= (RING_BUF_SIZE-1))	// End of Ring Buffer
    244          		pWriteBufPos = 0;
    245          
    246          	memcpy(&ring_buf[pWriteBufPos], (u8*)&RxMsg.ExtId, 4);
    247          
    248          	pWriteBufPos += 4;
    249          
    250          	memcpy(&ring_buf[pWriteBufPos], (u8*)&RxMsg.Data, 8);
    251          	
    252          	pWriteBufPos += 8;	
    253          
    254          	if(RxMsg.ExtId==0x18fff7dd || RxMsg.ExtId==0x18fff747) // rcu status
    255          	{
    256          		memcpy( &rx_Realy_Control, (u8*)&RxMsg.Data, 8);
    257          	}
    258          
    259          	else if(RxMsg.ExtId==0x18EFFF47 || RxMsg.ExtId==0x18EF2847) // auto_position
    260          	{
    261          		if(RxMsg.Data[0] == 124)
    262          		{
    263          			memcpy( &rx_Auto_position_Status, (u8*)&RxMsg.Data, 8);
    264          		}
    265          		
    266          	}
    267          	else if(RxMsg.ExtId==0x18ff9447) // lamp
    268          	{
    269          		memcpy( &RX_HCEPGN_65428, (u8*)&RxMsg.Data, 8);
    270          	}
    271          	else if(RxMsg.ExtId == 0x18ffaa47)	// Weighing System (Work Load Lamp)
    272          	{
    273          		memcpy( &rx_Weighing_System_Status, (u8*)&RxMsg.Data, 8);
    274          	}
    275          	
    276          	else if(RxMsg.ExtId == 0x18ffede4)	// EHCU
    277          	{
    278          		memcpy( &rx_EHCU_Status, (u8*)&RxMsg.Data, 8);
    279          	}
    280          	USART_ITConfig(USART2, USART_IT_TXE, ENABLE);
    281          	
    282          }
