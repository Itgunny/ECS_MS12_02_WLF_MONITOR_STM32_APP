    420          /**
    421            * @brief  This function handles SysTick Handler.
    422            * @param  None
    423            * @retval None
    424            */
    425          void SysTick_Handler(void)
    426          {
    427            	TimeDelay_Decrement();
    428          
    429              ++WL9FM_TIME.Cnt_1mSec;
    430          
    431          	if (WL9FM_TIME.Cnt_1mSec % 1 == 0)                   //  1msec
    432          	{
    433                  WL9FM_TIME.Flag_1mSec = 1;
    434          
    435          		if (WL9FM_TIME.Cnt_1mSec % 10 == 0)              //  10msec
    436          		{
    437          			WL9FM_TIME.Flag_10mSec = 1;
    438          			// ++, 160511 bwk
    439          			if(Hardware_Revision >= REVH)
    440          				CPU_OK();
    441          			// --, 160511 bwk
    442          
    443          			if (WL9FM_TIME.Cnt_1mSec % 100 == 0)         //  100msec
    444          			{
    445          				WL9FM_TIME.Flag_100mSec = 1;
    446          
    447          				if(WL9FM_TIME.Cnt_1mSec % 500 == 0)			// 500msec
    448          				{
    449          					WL9FM_TIME.Flag_500mSec = 1;
    450          					if (WL9FM_TIME.Cnt_1mSec % 1000 == 0)    //  1000 msec
    451          					{
    452          						WL9FM_TIME.Flag_1Sec = 1;
    453          						WL9FM_TIME.Cnt_1mSec = 0;
    454          					}
    455          				}
    456          
    457          				
    458          			}
    459          		}
    460          	}    
    461          }
