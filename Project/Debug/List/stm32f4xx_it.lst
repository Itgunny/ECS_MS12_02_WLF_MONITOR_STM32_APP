    535          /**
    536            * @brief  This function handles TIM4 global interrupt request.
    537            * @param  None
    538            * @retval None
    539            */
    540          void TIM4_IRQHandler(void)  //  10msec Timer / TimeBase UP Counter
    541          {
    542              TIM_ClearITPendingBit(TIM4, TIM_IT_Update);
    543              
    544              //  BUZZER Status가 "2" 일 경우 BUZZER OnTime 만큼 BUZZER On
    545              if (WL9FM_BUZZER.Status == 2)
    546              {
    547                  if (WL9FM_BUZZER.OnCnt++ < WL9FM_BUZZER.OnTime)
    548                  {
    549                      Buzzer_On();
    550                  }
    551                  else
    552                  {
    553                      WL9FM_BUZZER.Status = 0;               
    554                  }
    555              }        
    556              //  BUZZER Status가 "1" 일 경우 UnLimit BUZZER On
    557              else if (WL9FM_BUZZER.Status == 1)
    558              {
    559                  Buzzer_On();    
    560              }    
