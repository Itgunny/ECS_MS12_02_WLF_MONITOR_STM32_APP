    560          void TIM4_IRQHandler(void)  //  10msec Timer / TimeBase UP Counter
    561          {
    562              TIM_ClearITPendingBit(TIM4, TIM_IT_Update);
    563              
    564              //  BUZZER Status가 "2" 일 경우 BUZZER OnTime 만큼 BUZZER On
    565              if (WL9FM_BUZZER.Status == 2)
    566              {
