    391            * @brief  This function handles SysTick Handler.
    392            * @param  None
    393            * @retval None
    394            */
    395          void SysTick_Handler(void)
    396          {
    397            	TimeDelay_Decrement();
    398          
    399              ++WL9FM_TIME.Cnt_1mSec;
    400          
    401          	if (WL9FM_TIME.Cnt_1mSec % 1 == 0)                   //  1msec
    402          	{
    403                  WL9FM_TIME.Flag_1mSec = 1;
    404          
    405          		if (WL9FM_TIME.Cnt_1mSec % 10 == 0)              //  10msec
    406          		{
    407          			WL9FM_TIME.Flag_10mSec = 1;
    408          
    409          			if (WL9FM_TIME.Cnt_1mSec % 100 == 0)         //  100msec
    410          			{
    411          				WL9FM_TIME.Flag_100mSec = 1;
    412          
    413          				if(WL9FM_TIME.Cnt_1mSec % 500 == 0)			// 500msec
    414          				{
    415          					WL9FM_TIME.Flag_500mSec = 1;
    416          					if (WL9FM_TIME.Cnt_1mSec % 1000 == 0)    //  1000 msec
    417          					{
    418          						WL9FM_TIME.Flag_1Sec = 1;
    419          						WL9FM_TIME.Cnt_1mSec = 0;
    420          					}
    421          				}
    422          
    423          				
    424          			}
    425          		}
    426          	}    
    427          }
