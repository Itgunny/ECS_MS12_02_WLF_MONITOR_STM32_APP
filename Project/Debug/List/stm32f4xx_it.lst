    471          /**
    472            * @brief  This function handles CAN1 global interrupt request.
    473            * @param  None
    474            * @retval None
    475            */
    476          void CAN1_RX0_IRQHandler(void)
    477          {
    478          	u32 PF; 
    479          
    480          	CAN_Receive(CAN1,CAN_FIFO0,&RxMsg);
    481          				
    482          	Iden.Source_Address = (RxMsg.ExtId & 0x000000ff)  >> 0;
    483          	Iden.PDU_Specific = (RxMsg.ExtId  & 0x0000ff00) >> 8;
    484          	
    485          	// Iden.Source_Address == 71	 -->>	MCU
    486          	// Iden.Source_Address == 228	 -->>	EHCU	
    487          	// Iden.Source_Address == 0x29	 -->>	Smart Key
    488          	// Iden.Source_Address == 23	 -->>	Cluster
    489          	// Iden.Source_Address == 221	 -->>	RCU
    490          	// Iden.Source_Address == 0	 -->>	ECM
    491          	// Iden.Source_Address == 3	 -->>	TCU
    492          	
    493          	if((Iden.Source_Address == 0x47) || (Iden.Source_Address == 0x17) || (Iden.Source_Address == 0x29) || 
    494          		(Iden.Source_Address == 0xE4) || (Iden.Source_Address == 0xDD)|| (Iden.Source_Address == 0x4a)|| (Iden.Source_Address == 0xf4)
    495          		|| (Iden.Source_Address == 0x00)|| (Iden.Source_Address == 0x03) || (Iden.Source_Address == 0x02))
    496          		{
    497          	
    498          			if((PF == 254) || (PF == 255) || (PF == 239) )
    499          			{	
    500          				PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    501          				if(Iden.PDU_Specific == 251)
    502          				{
    503          					MoniInfoSendCnt = 0;
    504          					Flag_1Sec_MoniInfo = 0;
    505          					MoniInfoTotalPacketNum = 0;
    506          				}
    507          				else if(Iden.PDU_Specific == 232)	// Smart Key
    508          				{
    509          					smk_flag_data.recv_resp_packet |= RESPONSE_AUTHENTICATION;	
    510          					//smk_flag_data.recv_resp_packet |= 0x0100; 
    511          
    512          					memcpy((u8*)&recv_smartkey, (u8*)&RxMsg.Data[0], 8);
    513          				}
    514          				else
    515          				{
    516          	
    517          					//if(Iden.PDU_Specific == 0x9b)
    518          						OperateRingBuffer();
    519          				}
    520          			}
    521          			
    522          			else
    523          			{                    
    524                          OperateRingBuffer();
    525          			}
    526          	
    527          			CommErrCnt = 0;
    528          
    529          			
    530          			
    531          		}
    532          
    533          }
