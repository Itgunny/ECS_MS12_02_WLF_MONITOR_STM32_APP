    449          /**
    450            * @brief  This function handles CAN1 global interrupt request.
    451            * @param  None
    452            * @retval None
    453            */
    454          void CAN1_RX0_IRQHandler(void)
    455          {
    456          	u32 PF; 
    457          	u32 PS;
    458          	u32 PGN;
    459          
    460          	CAN_Receive(CAN1,CAN_FIFO0,&RxMsg);
    461          				
    462          	Iden.Source_Address = (RxMsg.ExtId & 0x000000ff)  >> 0;
    463          	Iden.PDU_Specific = (RxMsg.ExtId  & 0x0000ff00) >> 8;
    464          	
    465          	// Iden.Source_Address == 71	 -->>	MCU
    466          	// Iden.Source_Address == 228	 -->>	EHCU	
    467          	// Iden.Source_Address == 0x29	 -->>	Smart Key
    468          	// Iden.Source_Address == 23	 -->>	Cluster
    469          	// Iden.Source_Address == 221	 -->>	RCU
    470          	// Iden.Source_Address == 0	 -->>	ECM
    471          	// Iden.Source_Address == 3	 -->>	TCU
    472          	
    473          	if((Iden.Source_Address == 0x47) || (Iden.Source_Address == 0x17) || (Iden.Source_Address == 0x29) || 
    474          		(Iden.Source_Address == 0xE4) || (Iden.Source_Address == 0xDD)|| (Iden.Source_Address == 0x4a)|| (Iden.Source_Address == 0xf4)
    475          		|| (Iden.Source_Address == 0x00)|| (Iden.Source_Address == 0x03) || (Iden.Source_Address == 0x02))
    476          		{
    477          		#if 1
    478          			if(CANUpdateFlag == 0)
    479          			{
    480          				if((PF == 254) || (PF == 255) || (PF == 239) )
    481          				{	
    482          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    483          					if(Iden.PDU_Specific == 232)	// Smart Key
    484          					{
    485          						smk_flag_data.recv_resp_packet |= RESPONSE_AUTHENTICATION;	
    486          						//smk_flag_data.recv_resp_packet |= 0x0100; 
    487          
    488          						memcpy((u8*)&recv_smartkey, (u8*)&RxMsg.Data[0], 8);
    489          					}
    490          					else
    491          					{
    492          		
    493          						//if(Iden.PDU_Specific == 0x9b)
    494          							CAN_RX(RxMsg);
    495          					}
    496          				}
    497          				
    498          				else
    499          				{                    
    500          	                		CAN_RX(RxMsg);
    501          				}
    502          		
    503          				CommErrCnt = 0;
    504          			}
    505          			else if(CANUpdateFlag == 1)
    506          			{
    507          				if(Iden.Source_Address == CANUpdateSA)
    508          				{
    509          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    510          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    511          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    512          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    513          					{
    514          						CAN_RX(RxMsg);
    515          					}
    516          
    517          				}
    518          				
    519          			}
    520          		#else
    521          			if(CANUpdateFlag == 1)
    522          			{
    523          				if(Iden.Source_Address == CANUpdateSA)
    524          				{
    525          					PF = (RxMsg.ExtId  & 0x00ff0000) >> 16;
    526          					PS = (RxMsg.ExtId  & 0x0000FF00) >> 8;
    527          					PGN = (RxMsg.ExtId  & 0x00FFFF00) >> 8;
    528          					if(PGN == 0xEFFA || PGN  == 0xECFA || PGN  == 0xEBFA)
    529          					{
    530          						//OperateRingBuffer();
    531          						CAN_RX(RxMsg);
    532          						CANRXIndex++;
    533          					}
    534          
    535          				}
    536          				
    537          			}
    538          		#endif
    539          			
    540          			
    541          
    542          			
    543          			
    544          		}
    545          
    546          }
