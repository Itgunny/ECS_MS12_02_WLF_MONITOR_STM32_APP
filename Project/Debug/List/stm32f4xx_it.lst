    412          /**
    413            * @brief  This function handles SysTick Handler.
    414            * @param  None
    415            * @retval None
    416            */
    417          void SysTick_Handler(void)
    418          {
    419            	TimeDelay_Decrement();
    420          
    421              ++WL9FM_TIME.Cnt_1mSec;
    422          
    423          	if (WL9FM_TIME.Cnt_1mSec % 1 == 0)                   //  1msec
    424          	{
    425                  WL9FM_TIME.Flag_1mSec = 1;
    426          
    427          		if (WL9FM_TIME.Cnt_1mSec % 10 == 0)              //  10msec
    428          		{
    429          			WL9FM_TIME.Flag_10mSec = 1;
    430          
    431          			if (WL9FM_TIME.Cnt_1mSec % 100 == 0)         //  100msec
    432          			{
    433          				WL9FM_TIME.Flag_100mSec = 1;
    434          
    435          				if(WL9FM_TIME.Cnt_1mSec % 500 == 0)			// 500msec
    436          				{
    437          					WL9FM_TIME.Flag_500mSec = 1;
    438          					if (WL9FM_TIME.Cnt_1mSec % 1000 == 0)    //  1000 msec
    439          					{
    440          						WL9FM_TIME.Flag_1Sec = 1;
    441          						WL9FM_TIME.Cnt_1mSec = 0;
    442          					}
    443          				}
    444          
    445          				
    446          			}
    447          		}
    448          	}    
    449          }
