    561          	{
    562          		Uart2_SerialRxMsg[Uart2_SerialRxCnt] = (u8)USART_ReceiveData(USART2);
    563          
    564          		  //  디버깅할 때만 사용할 것
    565          		#ifdef DEBUG_CAN_RX
