    596                  }
    597                  else
    598                  {
    599                      WL9FM_BUZZER.Status = 0;               
    600                  }
    601              }        
    602              //  BUZZER Status가 "1" 일 경우 UnLimit BUZZER On
    603              else if (WL9FM_BUZZER.Status == 1)
