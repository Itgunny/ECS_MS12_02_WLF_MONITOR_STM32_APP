    250          void OperateRingBuffer(void)
    251          {
    252          	if(pWriteBufPos >= (RING_BUF_SIZE-1))	// End of Ring Buffer
    253          		pWriteBufPos = 0;
    254          
    255          	memcpy(&ring_buf[pWriteBufPos], (u8*)&RxMsg.ExtId, 4);
    256          
    257          	pWriteBufPos += 4;
    258          
    259          	memcpy(&ring_buf[pWriteBufPos], (u8*)&RxMsg.Data, 8);
    260          	
    261          	pWriteBufPos += 8;	
    262          
    263          	USART_ITConfig(USART2, USART_IT_TXE, ENABLE);
    264          	
    265          }
    266          void RTCSend(void)
    267          {
    268          	if(pWriteBufPos >= (RING_BUF_SIZE-1)) // End of Ring Buffer
    269          		pWriteBufPos = 0;
    270          
    271          	memcpy(&ring_buf[pWriteBufPos], (u8*)&SerialMsgRTC[0], 12);
    272          
    273          	pWriteBufPos += 12;
    274          
    275          	USART_ITConfig(USART2, USART_IT_TXE, ENABLE);
    276          }
